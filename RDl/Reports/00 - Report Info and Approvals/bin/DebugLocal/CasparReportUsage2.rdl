  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@TimeInput">
            <Value>=Parameters!TimeInput.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @TimeSince AS DAte
SET @TimeSince = @TimeInput


 IF OBJECT_ID('tempdb..#gh1') IS NOT NULL DROP TABLE #gh1
 IF OBJECT_ID('tempdb..#gh2') IS NOT NULL DROP TABLE #gh2
 IF OBJECT_ID('tempdb..#gh3') IS NOT NULL DROP TABLE #gh3
 IF OBJECT_ID('tempdb..#gh4') IS NOT NULL DROP TABLE #gh4

--  First Split Of the Path field in order to get media, category, etc ---------------------
select 			
			ItemID,
			Name,
			Description,
			--SUBSTRING(Cat.Path,CHARINDEX('/',Cat.Path)+1,       CHARINDEX('r/',Cat.Path)-1) AS [Report Manager],
			SUBSTRING(Cat.Path,CHARINDEX('r/',Cat.Path)+2,       100) AS [Temp1]
into #GH1 	
from		Catalog_hist Cat
where		Cat.Name LIKE 'ACT%' 		
	

--  Second Split Of the Path field in order to get media, category, etc ---------------------	
Select 
		ItemID,
		Name,
		Description,
		SUBSTRING(#GH1.Temp1, 1, CHARINDEX('/',#GH1.Temp1)-1) AS [Layer],
		SUBSTRING(#GH1.Temp1, CHARINDEX('/',#GH1.Temp1)+1,      100) AS [Temp2]
INTO #GH2
from #GH1
DROP TABLE #GH1

--  Third Split Of the Path field in order to get media, category, etc ---------------------	
Select	
		ItemID,
		Name,
		Description,
		[Layer],
		SUBSTRING(#GH2.Temp2, 1, CHARINDEX('/',#GH2.Temp2)-1) AS [Category]
INTO #GH3
from #gh2
DROP TABLE #GH2


-- Get Count Since ---------------------------------------

select	ReportID,
		--UserName,
		COUNT(Status) AS [Times Run]
INTO #gh4
from ExecutionLog
where TimeEnd &gt; @TimeSince
Group By ReportID --,UserName
----------------------------------------------------------



---------Final Select Query-------------------------------------------------
select 

		Name,
		Description,
		[Layer],
		[Category],
		[Times Run],
		@TimeSince AS [Run From Date]
		
				
from	( 
		#gh3
		LEFT OUTER JOIN #gh4 gh4 ON gh4.ReportID = #gh3.ItemID 
		)




UNION ALL

select 
		--Cat.ItemID,Exl.TimeEnd,
		Cat.Name, 
		Cat.Description, 
		'None' AS [Layer],
		'Reconciliation' AS [Category],
		COUNT(Exl.Status) AS [Times Run],
		@TimeSince AS [Run From Date]
		

FROm	Catalog_hist Cat
		LEFT OUTER JOIN ExecutionLog Exl ON Exl.ReportID = Cat.ItemID 
Where Cat.Name LIKE 'REC0%' AND @TimeSince &lt; Exl.TimeEnd
GROUP BY 	Cat.Name,Cat.Description





ORDER BY [Name],[Layer]</CommandText>
