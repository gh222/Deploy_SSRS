				select New_Sequence_Number as Sequence_Number,UDF_Type,contract_id,Contract_Export_ID,Contract_Description,Multi_Particpant,udf_label,udf_sid,UDF_Name,contract_sid,contact_sid,user_contact_sid,admin_class,Contract_Status,Contributor_ID,Contributor_Name,Sort_order_nbr+1 as Sort_order_nbr,UDF_Value,udkey_1_sid,udkey_1_group_sid,udkey_2_sid,udkey_2_group_sid,udkey_3_sid,udkey_3_group_sid,udkey_4_sid,udkey_4_group_sid,udkey_5_sid,udkey_5_group_sid,udkey_6_sid,udkey_6_group_sid,udkey_7_sid,udkey_7_group_sid,udkey_8_sid,udkey_8_group_sid,udkey_9_sid,udkey_9_group_sid,udkey_10_sid,udkey_10_group_sid,udkey_12_sid,udkey_12_group_sid,udkey_14_sid,udkey_14_group_sid,Royalty_Period_From_sid,Royalty_Period_To_sid,Sales_Period_From_sid,Sales_Period_To_sid,Other_Period_From_sid,Other_Period_To_sid,Royalty_Period_From,Royalty_Period_To,Sales_Period_From,Sales_Period_To,Other_Period_From,Other_Period_To 
				from #CTE_Keep_Highest_Sequence cte
		
		END
	
		
	/**************************************************************************************************************************************************************************************/
	/*********************** Templates UDF Values *****************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/

			IF (LEFT(@UDF_Name_Orginal,9) LIKE '_Template') 
			BEGIN 

					DECLARE @Approval_Group_sid int
					IF (@UDF_Name_Orginal = '_Template_Group_Default') BEGIN SET @Approval_Group_sid = 0 END
					IF (@UDF_Name_Orginal = '_Template_Group_Advance') BEGIN SET @Approval_Group_sid = 1 END
					IF (@UDF_Name_Orginal = '_Template_Group_Accruals') BEGIN SET @Approval_Group_sid = 2 END
					IF (@UDF_Name_Orginal = '_Template_Group_Accruals_39') BEGIN SET @Approval_Group_sid = 5 END
					IF (@UDF_Name_Orginal = '_Template_Group_Statements') BEGIN SET @Approval_Group_sid = 3 END

					INSERT INTO #x_contract_udf (UDF_Type,udf_label,contract_sid,UDF_Value,contract_id, Contract_Description)
					select 
						'Contract Default' AS UDF_Type
						, @UDF_Name_Orginal as udf_label
						,xc.contract_sid
						,ctgh.descr As UDF_Value
						,xc.contract_id, xc.Contract_Description
					from			x_deal						xd
					INNER JOIN		#x_contract xc on xc.contract_sid = xd.contract_sid
					LEFT JOIN		c_template_group_hdr		ctgh on ctgh.template_group_sid = xd.template_group_sid
					where xd.approval_group_sid  = @Approval_Group_sid
			END

	/**********************************************************************************************/
	/*********************** Statement Schedule  **************************************************/
	/**********************************************************************************************/
		
		IF (@UDF_Name_Orginal = '_Statement_Schedule')
		BEGIN
				UPDATE #x_contract_udf
				SET Udf_value = CAST(xcs.sort_order_nbr as nvarchar(3)) + '\'+ CAST(xcs.no_of_periods as nvarchar(10)) + '\' + xpt.period_type_id+ '\' + CAST(xcs.mailing_deadline_nbr as nvarchar(3)) + '\' + ISNULL(xcs.comment,'No Comment') +  '\'  + CAST(xcs.actual_periods_scheduled as nvarchar(3))			, UDF_Label = @UDF_Name_Orginal
				from x_contract_schedule xcs
				INNER JOIN #x_contract xc on xc.contract_sid = xcs.contract_sid
				INNER JOIN [x_period_type] xpt on xpt.period_type_sid = xcs.period_type_sid
		END
	
	/*******************************************************************************************************************************************************************************************/
	/*********************** Section Group  **************************************************************************************************************************************************/
	/*******************************************************************************************************************************************************************************************/
	
		IF (@UDF_Name_Orginal = '_Section_Group')
		BEGIN
				UPDATE #x_contract_udf
				SET Udf_value = csgh.descr, UDF_Label = @UDF_Name_Orginal
				--SELECT  xc.contract_id,csgh.descr AS UDF_Value
				FROM #x_contract xc
				INNER JOIN x_deal xd ON xc.contract_sid = xd.contract_sid
				INNER JOIN c_section_group_hdr csgh ON xd.section_group_sid = csgh.section_group_sid


		END

	/*******************************************************************************************************************************************************************************************/
	/*********************** Where UDF_Value is Number, but should be a string *****************************************************************************************************************/
	/*******************************************************************************************************************************************************************************************/

		IF (@UDF_Name_Orginal IN ('_All','Rolling Reserve Period Type','Period Interval','Principal Product', 'Contribution Type', 'Pay Currency', 'Last Period for MU Escalation','Last Period for Equ Escalation','ISM 2005 Start Sales Period','First Alliant Statement Month', 'LOTCAPS Contract Header Type','Calculate Reserve?','Buyout Trigger','Buyout Type','Calculation Type','Post Buyout Type', 'PPO Contract ?', 'PACT Revenue Source'))
		BEGIN

		/* 'Rolling Reserve Period Type' */		UPDATE #x_contract_udf 	set UDF_Value = LEFT(xpt.descr, 30)			/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		x_period_type			xpt	 on xcu.UDF_Value = xpt.period_type_sid where xcu.udf_label IN ('Rolling Reserve Period Type','Period Interval') and ISNUMERIC(xcu.UDF_Value) =1
		/* 'Principal Product'	*/				UPDATE #x_contract_udf 	set UDF_Value = LEFT(cu1.udkey_1_id, 30)	/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		c_udkey_1			cu1	 on xcu.UDF_Value = cu1.udkey_1_sid where xcu.udf_label IN ('Principal Product') and ISNUMERIC(xcu.UDF_Value) =1
		/* 'Contribution Type'	*/				UPDATE #x_contract_udf 	set UDF_Value = LEFT(cu8.udkey_8_id, 30)	/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		c_udkey_8			cu8	 on xcu.UDF_Value = cu8.udkey_8_sid where xcu.udf_label IN ('Contribution Type')and ISNUMERIC(xcu.UDF_Value) =1
		/* 'Pay Currency'	*/					UPDATE #x_contract_udf 	set UDF_Value = LEFT(cu10.udkey_10_id, 30)	/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		c_udkey_10			cu10	 on xcu.UDF_Value = cu10.udkey_10_sid where xcu.udf_label IN ('Pay Currency')and ISNUMERIC(xcu.UDF_Value) =1
		/* 'Last Period for MU Escalation'	*/	UPDATE #x_contract_udf 	set UDF_Value = LEFT(xp.period_id, 30)		/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		x_period			xp	 on xcu.UDF_Value = xp.period_sid where xcu.udf_label IN ('Last Period for MU Escalation','Last Period for Equ Escalation','ISM 2005 Start Sales Period','First Alliant Statement Month')and ISNUMERIC(xcu.UDF_Value) =1
		/* 'LOTCAPS Contract Header Type'	*/	UPDATE #x_contract_udf 	set UDF_Value = LEFT(cdev.descr, 30)		/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		c_datatype_enumerated_value		cdev	 on xcu.UDF_Value = cdev.enumerated_sid where xcu.udf_label IN ('LOTCAPS Contract Header Type','Calculate Reserve?','Buyout Trigger','Buyout Type','Calculation Type','Post Buyout Type')and ISNUMERIC(xcu.UDF_Value) =1
		/* 'PPO Contract ?'	*/					UPDATE #x_contract_udf 	set UDF_Value = LEFT(cu3.udkey_3_id, 30)	/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		c_udkey_3		cu3	 on xcu.UDF_Value = cu3.udkey_3_sid where xcu.udf_label IN ('PPO Contract ?')and ISNUMERIC(xcu.UDF_Value) =1
		/* 'PACT Revenue Source'	*/			UPDATE #x_contract_udf 	set UDF_Value = LEFT(cu9.udkey_9_id, 30)	/*select xcu.UDF_Value, xpt.descr*/ from	#x_contract_udf			xcu		INNER JOIN		c_udkey_9		cu9	 on xcu.UDF_Value = cu9.udkey_9_sid where xcu.udf_label IN ('PACT Revenue Source')and ISNUMERIC(xcu.UDF_Value) =1
		
		END


		IF (@UDF_Name_Orginal IN ('Accrual start date','Accrual stop date','Advance start date','Advance stop date','Statement start date','Statement stop date'))
		BEGIN
			DECLARE @Deal_Type nvarchar(10)			SET @Deal_Type = ''
			DECLARE @Start_Stop nvarchar(10)		SET @Start_Stop = ''
			DECLARE @Start_Stop_Field nvarchar(10)	SET @Start_Stop_Field = ''

			IF ( LEFT(@UDF_Name_Orginal,3)='Acc') BEGIN SET @Deal_Type = 'ACC'   END
			IF ( LEFT(@UDF_Name_Orginal,3)='Sta') BEGIN SET @Deal_Type = 'STA'   END
			IF ( LEFT(@UDF_Name_Orginal,3)='Adv') BEGIN SET @Deal_Type = 'ADV'   END
			IF (@UDF_Name_Orginal LIKE '%Start%') BEGIN SET @Start_Stop = '52'  END
			IF (@UDF_Name_Orginal LIKE '%Stop%') BEGIN SET @Start_Stop = '53'  END
			IF (@UDF_Name_Orginal LIKE '%Start%') BEGIN SET @Start_Stop_Field = 'xp1'  END
			IF (@UDF_Name_Orginal LIKE '%Stop%') BEGIN SET @Start_Stop_Field = 'xp2'  END

			SET @SQL = 
			'
			UPDATE  #x_contract_udf SET   Udf_value = '+@Start_Stop_Field+'.period_id+''-15''
			from		x_deal				xd 
			INNER JOIN #x_contract_udf		xcu on xd.contract_sid = xcu.contract_sid
			LEFT JOIN x_period				xp1 on xp1.period_sid = xd.actual_first_pd_typ_period_sid
			LEFT JOIN x_period				xp2 on xp2.period_sid = xd.target_expires_period_sid
			where xcu.UDF_Value is null AND deal_id LIKE ''%'+@Deal_Type+'%'' AND xd.status_sid = '''+@Start_Stop+'''
			'
			exec (@SQL)
		END

	/*******************************************************************************************************************************************************************************************/
	/*********************** _Last_Approved_Statement *****************************************************************************************************************/
	/*******************************************************************************************************************************************************************************************/


		IF(@UDF_Name_Orginal			LIKE	'_Last_Approved_Statement')
		BEGIN

		;WITH CTE_Last_Approved AS
		(
		select xd.deal_sid, xtjqh.due_datetime, xtjqh.actual_run_date, xa.approved_datetime, ROW_NUMBER() OVER (PARTITION BY xd.deal_sid ORDER BY xa.approved_datetime desc) AS Roww
		, xp.period_id, xc.contract_sid
		from x_template_job_queue_hist xtjqh
		INNER JOIN x_deal xd on xd.deal_sid = xtjqh.deal_sid and xd.deal_id LIKE '%ST%' 
		INNER JOIN #x_contract xc on xc.contract_sid = xd.contract_sid
		INNER JOIN x_approval xa on xa.approval_sid = xtjqh.approval_sid
		INNER JOIN x_period xp on xp.period_sid = xtjqh.processing_cutoff_period_sid
		where xtjqh.due_datetime &gt; (GETDATE()-363*2)
		)
		insert into #x_contract_udf (contract_sid,UDF_Type,UDF_Label,UDF_Descr,UDF_Value)
		select contract_sid	,'Contract DropDown' as UDF_Type,'_Last_Approved_Statement' as UDF_Label,'_Last_Approved_Statement' as UDF_Descr,'Due '+CAST(CAST(due_datetime as date) as nvarchar(10))+	' | Run '+CAST(CAST(actual_run_date as date) as nvarchar(10))	+' | Approved '+CAST( CAST(approved_datetime as date) as nvarchar(10))+' | Period '+	period_id as UDF_Value
		from CTE_Last_Approved cte
		where Roww = 1

		
		END

