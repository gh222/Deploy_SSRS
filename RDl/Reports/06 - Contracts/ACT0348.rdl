	/**************************************************************************************************************************************************************************************/
			
			
			UPDATE #x_contract_udf		SET Contributor_Specific = 1		FROM #x_contract_udf	where Multi_Particpant = 1		AND udf_sid IN (202,206,207,209,211,215 ,217,206,141,209,184,217,151,212,8,207)
			UPDATE #x_contract_udf		SET Contributor_Specific = 1		FROM #x_contract_udf	where Multi_Particpant = 0	
			UPDATE #x_contract_udf
			SET Contract_Export_ID =  (CASE 
											WHEN							  Multi_Particpant = 0 THEN Contract_ID 
											WHEN Contributor_Specific = 1 AND Multi_Particpant = 1 AND Sequence_Number &lt;&gt; 0 AND LEN(Sequence_Number) = 1  THEN Contract_ID + '00' +Sequence_Number
											WHEN Contributor_Specific = 1 AND Multi_Particpant = 1 AND Sequence_Number &lt;&gt; 0 AND LEN(Sequence_Number) = 2  THEN Contract_ID + '0' +Sequence_Number
											WHEN Contributor_Specific = 1 AND Multi_Particpant = 1 AND Sequence_Number &lt;&gt; 0 AND LEN(Sequence_Number) = 3  THEN Contract_ID  +Sequence_Number
											 ELSE NULL END) 

