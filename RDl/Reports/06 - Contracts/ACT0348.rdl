			SET Royalty_Period_To = xp.period_id
			from #x_contract_udf xc
			LEFT JOIN	x_period  xp on xc.Royalty_Period_To_sid = xp.period_sid

			UPDATE #x_contract_udf
			SET Sales_Period_From = xp.period_id
			--select xp.period_id
			from #x_contract_udf xc
			LEFT JOIN	x_period  xp on xc.Sales_Period_From_sid = xp.period_sid

			UPDATE #x_contract_udf
			SET Sales_Period_To = xp.period_id
			from #x_contract_udf xc
			LEFT JOIN	x_period  xp on xc.Sales_Period_To_sid = xp.period_sid
		
			UPDATE #x_contract_udf
			SET Other_Period_From = xp.period_id
			--select xp.period_id
			from #x_contract_udf xc
			LEFT JOIN	x_period  xp on xc.Other_Period_From_sid = xp.period_sid

			UPDATE #x_contract_udf
			SET Other_Period_To = xp.period_id
			from #x_contract_udf xc
			LEFT JOIN	x_period  xp on xc.Other_Period_To_sid = xp.period_sid

