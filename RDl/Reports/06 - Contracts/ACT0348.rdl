		,	xcu.udkey_7_sid 			,	xcu.udkey_7_group_sid	 			 
		,	xcu.udkey_8_sid 			,	xcu.udkey_8_group_sid	 		
		,	xcu.udkey_9_sid 			,	xcu.udkey_9_group_sid	 			
		,	xcu.udkey_10_sid 			,	xcu.udkey_10_group_sid	 			  
