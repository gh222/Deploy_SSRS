	UNION ALL	select '_Scope_Next_Rights' as UDF_Name
	UNION ALL	select '_Product_Share_Not_Available' as UDF_Name
	UNION ALL	select '_Product_Share_Next' as UDF_Name
	UNION ALL	select '_Status' as UDF_Name
	UNION ALL	select '_Buyout_Expiration_Date' as UDF_Name
	UNION ALL	select '_Statement_Schedule' as UDF_Name
	UNION ALL	select '_Section_Group' as UDF_Name
	UNION ALL	select '_Last_Approved_Statement' as UDF_Name

select UDF_Name	from #udf_Possible_To_Select ORDER BY CASE WHEN UDF_Name LIKE '_All' THEN '0' WHEN LEFT(UDF_Name,1) = '_' THEN '1'	 ELSE UDF_Name END ASC</CommandText>
      </Query>
      <Fields>
