			SET admin_class = xa.admin_class_id
			from #x_contract xc
			INNER JOIN #x_contract_udf	xcu on xcu.contract_sid = xc.contract_sid
			LEFT JOIN	x_admin_class xa   on xa.admin_class_sid = xc.admin_class_sid 
			


	/**********************************************************************************************/
