	IF OBJECT_ID('tempdb..#x_contract_udf') IS NOT NULL DROP TABLE #x_contract_udf
	CREATE TABLE #x_contract_udf (	UDF_Type				 nvarchar(60),	contract_id				 nvarchar(60),			Contract_Export_ID		 nvarchar(60),
									Contract_Description	nvarchar(60),	Multi_Particpant		bit,					udf_label				nvarchar(100),
									udf_sid					nvarchar(60),	UDF_Name nvarchar(30), contract_sid			 nvarchar(60) ,			contact_sid				 nvarchar(60) DEFAULT '0',
									user_contact_sid		 nvarchar(60) DEFAULT '0',										admin_class			nvarchar(60),
									Contract_Status			nvarchar(60),	Contributor_ID nvarchar(30),					Contributor_Name nvarchar(30), Sequence_Number			 nvarchar(60),
									Sort_order_nbr nvarchar(60),			UDF_Value				 nvarchar(600),			udkey_1_sid				nvarchar(60),
									udkey_1_group_sid		 nvarchar(60),	udkey_2_sid				nvarchar(60),			udkey_2_group_sid		 nvarchar(60),
									udkey_3_sid				nvarchar(60),	udkey_3_group_sid		 nvarchar(60),			udkey_4_sid				 nvarchar(60),
									udkey_4_group_sid		 nvarchar(60),	udkey_5_sid				 nvarchar(60),			udkey_5_group_sid		 nvarchar(60),
									udkey_6_sid				nvarchar(60),	udkey_6_group_sid		 nvarchar(60),			udkey_7_sid				 nvarchar(60),
									udkey_7_group_sid		 nvarchar(60),	udkey_8_sid				nvarchar(60),			udkey_8_group_sid		 nvarchar(60),	udkey_9_sid				nvarchar(60),
									udkey_9_group_sid		 nvarchar(60),	udkey_10_sid			 nvarchar(60),			udkey_10_group_sid		 nvarchar(60),	udkey_12_sid			nvarchar(60),			udkey_12_group_sid		 nvarchar(60),
										udkey_13_sid nvarchar(60), udkey_13_group_sid nvarchar(60),LOTCAPS_Resolved_Num int,
									
									udkey_14_sid			nvarchar(60),	udkey_14_group_sid		 nvarchar(60),			 
									Royalty_Period_From_sid int, Royalty_Period_To_sid int, Sales_Period_From_sid int,Sales_Period_To_sid int,  Other_Period_From_sid int, Other_Period_To_sid int,
									Royalty_Period_From nvarchar(60), Royalty_Period_To nvarchar(60), Sales_Period_From nvarchar(60),Sales_Period_To nvarchar(60),  Other_Period_From nvarchar(60), Other_Period_To nvarchar(60),
									UDF_Descr nvarchar(30), Contributor_Specific tinyint
								)
	
		SET @SQL = 
		'
		INSERT INTO #x_contract_udf (UDF_Type,  udf_label, udf_sid ,contract_sid,contract_id, Contract_Description , UDF_Name,UDF_Value,Sequence_Number,Multi_Particpant,Sort_order_nbr,UDF_Descr,Contributor_Specific)
		select ''Contract Default'' AS UDF_Type,   cu.udf_label, cu.udf_sid ,xc.contract_sid,xc.contract_id, xc.Contract_Description, LEFT(cu.UDF_Name,30 )  as UDF_Name, REPLACE(UDF_Value,'','','''') as UDF_Value, 0 as Sequence_Number,xc.Multi_Particpant, 0 as Sort_order_nbr,cu.UDF_Descr,0 as Contributor_Specific
		from			x_contract_udf		xcu
		INNER JOIN		#x_contract			xc on xc.contract_sid = xcu.contract_sid
		INNER JOIN		#c_udf				cu on cu.udf_sid = xcu.udf_sid
		'+@Payee_Filter_Default+'
		'
		exec (@SQL)
		


	/**************************************************************************************************************************************************************************************/
	/*********************** Gather UDF Dropdown ******************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
	
		DECLARE @Contributor_Filer nvarchar(400) SET @Contributor_Filer = ''
		DECLARE @Contributor_Filer_Field nvarchar(400) SET @Contributor_Filer_Field = ',xcu.user_contact_sid		as contract_sid  ,NULL as 		user_contact_sid'
		
		IF (@UDF_Name_Orginal IN ('_Artist_Shares','_Contributors', '_Recipient','_Buyout_Expiration_Date')) 
	
		BEGIN	
				SET @Contributor_Filer_Field = ',ISNULL(xcp.contact_sid,0)		as contract_sid  ,ISNULL(xcu.user_contact_sid,0) as 		user_contact_sid'	
				SET @Contributor_Filer = 'INNER JOIN		x_contract_participant		xcp on xc.contract_sid = xcp.contract_sid AND xc.contract_sid = xcp.contract_sid AND  xcp.contact_sid = ISNULL(xcu.user_contact_sid,xcp.contact_sid )' 
		END
		
		SET @SQL =
		'
		INSERT INTO #x_contract_udf (UDF_Type,udf_label,udf_sid,contract_sid,contact_sid,user_contact_sid,Sequence_Number,UDF_Value,udkey_1_sid,udkey_1_group_sid,udkey_2_sid,udkey_2_group_sid,udkey_3_sid,udkey_3_group_sid,udkey_4_sid,udkey_4_group_sid,udkey_5_sid,udkey_5_group_sid,udkey_6_sid,udkey_6_group_sid,udkey_7_sid,udkey_7_group_sid,udkey_8_sid,udkey_8_group_sid,udkey_9_sid,udkey_9_group_sid,udkey_10_sid,udkey_10_group_sid,udkey_12_sid,udkey_12_group_sid,udkey_13_sid	,udkey_13_group_sid, udkey_14_sid,udkey_14_group_sid,contract_id, Contract_Description,Royalty_Period_From_sid, Royalty_Period_To_sid, Sales_Period_From_sid,Sales_Period_To_sid,  Other_Period_From_sid, Other_Period_To_sid,Multi_Particpant,Udf_name,sort_order_nbr,UDF_Descr,Contributor_Specific)
