		IF (@UDF_Name_Orginal IN ('_All','Payee','LOTCAPS Address Code','Lotcaps Contract ID','LOTCAPS Resolved Slot Number','Payment Minimum','LOTCAPS Original Contract Date'))
			BEGIN
