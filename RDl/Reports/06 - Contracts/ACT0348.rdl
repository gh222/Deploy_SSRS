				-- select c.Contributor_ID, c.user_contact_sid,  c.Multi_Particpant,  c.Contributor_Name, xcu.*
				from			#x_contract_udf xcu
				LEFT JOIN		#Multi_Contributor_ID_2	c  on   xcu.contract_sid = c.contract_sid AND xcu.Sequence_Number = c.seq_nbr -- On Contract &amp; Sort Number
				where c.Multi_Particpant = 1 AND xcu.Contributor_Specific = 1 AND xcu.udf_sid = 202

