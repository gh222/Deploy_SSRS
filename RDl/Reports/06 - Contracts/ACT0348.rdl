	/*********************** Remove Equity Rates from Non Equity **********************************/
	/**********************************************************************************************/

		IF (@UDF_Name IN ('_All','ISM 2005 Start Sales Period','Buyout Group','MU Initial Fees Uplift','ISM Initial Fees Uplift','EQ/MU Royalty Rates WW 2003','Equity Royalty Rate','Equity/MU 3rd Party Rates','Equity/MU Standard Advance','Equity/MU WW Rates','Equity/MU/PACT Contrib Fees'))
		BEGIN	
			-- Remove Equity
			DELETE FROM #x_contract_udf
			from #x_contract_udf xcu
			INNER JOIN x_admin_class xac on xac.admin_class_id = xcu.admin_class
			where xac.admin_class_id NOT IN ('VIDEO - EM','VIDEO - EM2003','VIDEO - EMF','VIDEO - EMF2003','VIDEO - EQ MP') and udf_sid IN (select udf_sid from c_udf where label_id IN ('EQ/MU Royalty Rates WW 2003','Equity Royalty Rate','Equity/MU 3rd Party Rates','Equity/MU Standard Advance','Equity/MU WW Rates','Equity/MU/PACT Contrib Fees'))

			-- Remove ISM
			DELETE FROM #x_contract_udf
			from #x_contract_udf xcu
			INNER JOIN x_admin_class xac on xac.admin_class_id = xcu.admin_class
			where xac.admin_class_id NOT IN ('VIDEO - ISM','VIDEO - ISM2005') and udf_sid IN (181,1968681915, 1968681894)
			
			-- Remove MU
			DELETE FROM #x_contract_udf
			from #x_contract_udf xcu
			INNER JOIN x_admin_class xac on xac.admin_class_id = xcu.admin_class
			where xac.admin_class_id NOT IN ('VIDEO - MU AS','VIDEO - MU ASF','VIDEO - MU2012') and udf_sid = 1968681898 

			-- Remove MCPS
			DELETE FROM #x_contract_udf
			from #x_contract_udf xcu
			INNER JOIN x_admin_class xac on xac.admin_class_id = xcu.admin_class
			where xac.admin_class_id NOT IN ('MCPS','MCPS - %REV') and udf_name LIKE 'MCPS%'

		END

