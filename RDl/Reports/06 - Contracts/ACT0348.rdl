				DELETE FROM #x_contract_udf where udf_sid IN (207) AND user_contact_sid = 0 AND Multi_Particpant = 1 AND Contributor_Specific = 1
				DELETE from #x_contract_udf where Contributor_Specific = 1 AND Multi_Particpant = 1 AND   user_contact_sid = 0 And udf_sid = 202

