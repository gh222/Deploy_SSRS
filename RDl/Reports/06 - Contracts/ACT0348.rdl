		DECLARE @Direct_Input_Inclusion_Contract_Filter nvarchar(200)				SET @Direct_Input_Inclusion_Contract_Filter = ''
		DECLARE @Direct_Input_Inclusion_Product_Filter_On_Contract nvarchar(200)	SET @Direct_Input_Inclusion_Product_Filter_On_Contract = ''
		DECLARE @Direct_Input_Inclusion_Contributor_Filter nvarchar(200)			SET @Direct_Input_Inclusion_Contributor_Filter = ''


		IF(	@Direct_Input_Inclusion &lt;&gt; 'No')
		BEGIN
				SET @SQL = 
				'
				EXEC  '+@Warehoue_Supporting_Server_n_Database+'.[Report_Support].[usp_Truncate_Tables]  @Table_To_Truncate = ''ACT0348_Inclusion'' 
				EXEC  '+@Warehoue_Supporting_Server_n_Database+'.[Report_Support].[usp_Run_Package]  @Package_To_Run = ''Caspar_Warehouse_ACT0348_Inclusion''
				'
				EXEC (@SQL)

				IF OBJECT_ID('tempdb..#x_contract_Inclusion') IS NOT NULL DROP TABLE #x_contract_Inclusion
				CREATE TABLE #x_contract_Inclusion (Inclusion_Column nvarchar(100),Direct_Input_Inclusion nvarchar(30))

				SET @SQL = 
				'
				INSERT INTO #x_contract_Inclusion (Inclusion_Column )
				select distinct Inclusion_Column
				from '+@Warehoue_Supporting_Server_n_Database+'.[Report_Support].[ACT0348_Inclusion]
				'
				EXEC (@SQL)

				IF (@Direct_Input_Inclusion = 'Yes_Contract' )			BEGIN SET @Direct_Input_Inclusion_Contract_Filter					= ' INNER JOIN #x_contract_Inclusion xci on xci.Inclusion_Column = xc.contract_id ' END
				IF (@Direct_Input_Inclusion = 'Yes_Contributor_ID' )	BEGIN SET @Direct_Input_Inclusion_Contributor_Filter				= ' INNER JOIN #x_contract_Inclusion xci on xci.Inclusion_Column = cc.contact_ID ' END
				IF (@Direct_Input_Inclusion = 'Yes_Product_Scope' )		BEGIN SET @Direct_Input_Inclusion_Product_Filter_On_Contract		= ' INNER JOIN #x_contract_Product_filter xci on xci.contract_sid = xc.contract_sid ' 	 END
				--select * from #x_contract_Inclusion
		END


		/*****************************************************************************************************/
		/****************************** Exclusion Filter *****************************************************/
		/*****************************************************************************************************/


		DECLARE @Direct_Input_Exclusion_Contract_Filter nvarchar(200)				SET @Direct_Input_Exclusion_Contract_Filter = ''
		DECLARE @Direct_Input_Exclusion_Product_Filter_On_Contract nvarchar(200)	SET @Direct_Input_Exclusion_Product_Filter_On_Contract = ''
		DECLARE @Direct_Input_Exclusion_Contributor_Filter nvarchar(200)			SET @Direct_Input_Exclusion_Contributor_Filter = ''


		IF(	@Direct_Input_Exclusion &lt;&gt; 'No')
		BEGIN
				SET @SQL = 
				'
				EXEC  '+@Warehoue_Supporting_Server_n_Database+'.[Report_Support].[usp_Truncate_Tables]  @Table_To_Truncate = ''ACT0348_Exclusion'' 
				EXEC  '+@Warehoue_Supporting_Server_n_Database+'.[Report_Support].[usp_Run_Package]  @Package_To_Run = ''Caspar_Warehouse_ACT0348_Exclusion''
				'
				EXEC (@SQL)

				IF OBJECT_ID('tempdb..#x_contract_Exclusion') IS NOT NULL DROP TABLE #x_contract_Exclusion
				CREATE TABLE #x_contract_Exclusion (Exclusion_Column nvarchar(100),Direct_Input_Exclusion nvarchar(30))

				SET @SQL = 
				'
				INSERT INTO #x_contract_Exclusion (Exclusion_Column )
				select distinct Exclusion_Column
				from '+@Warehoue_Supporting_Server_n_Database+'.[Report_Support].[ACT0348_Exclusion]
				'
				EXEC (@SQL)

				IF (@Direct_Input_Exclusion = 'Yes_Contract' )			BEGIN SET @Direct_Input_Exclusion_Contract_Filter					= ' AND xc.contract_id NOT IN (select Exclusion_Column from #x_contract_Exclusion) ' END
				IF (@Direct_Input_Exclusion = 'Yes_Contributor_ID' )	BEGIN SET @Direct_Input_Exclusion_Contributor_Filter				= ' where cc.contact_id NOT IN (select Exclusion_column from #x_contract_Exclusion) ' END
		
		END



