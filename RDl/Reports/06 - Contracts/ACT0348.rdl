            <Value>=Parameters!Contributor_ID_Input.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Product_ID_Input">
            <Value>=Parameters!Product_ID_Input.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Keyword_Input">
            <Value>=Parameters!Keyword_Input.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Payee_Input">
            <Value>=Parameters!Payee_Input.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Report_Folder_Input">
            <Value>=Parameters!Report_Folder_Input.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>


-- SET SHOWPLAN_XML Off	


		DECLARE @Contract_ID nvarchar(60)					SET @Contract_ID				= @Contract_ID_Input
		DECLARE @UDF_Name nvarchar(600)						SET @UDF_Name					= @UDF_Name_Input
		DECLARE @Contributor_Name nvarchar(30)				SET @Contributor_Name			= @Contributor_Name_Input
		DECLARE @Contributor_ID nvarchar(30)				SET @Contributor_ID				= @Contributor_ID_Input
		DECLARE @Product_ID nvarchar(30)					SET @Product_ID					= @Product_ID_Input
		DECLARE @Keyword nvarchar(60)						SET @Keyword					= @Keyword_Input
		DECLARE @Payee nvarchar(60)							SET @Payee						= @Payee_Input
		DECLARE @Report_Folder	nvarchar(200)				SET @Report_Folder			= @Report_Folder_Input
	

		--DECLARE @Direct_Output_CheckBox bit					SET @Direct_Output_CheckBox		= @Direct_Output_CheckBox_Input
		--DECLARE @Direct_Input_Inclusion nvarchar(30)		SET @Direct_Input_Inclusion		= @Direct_Input_Inclusion_Input
		--DECLARE @Direct_Input_Exclusion nvarchar(30)		SET @Direct_Input_Exclusion		= @Direct_Input_Exclusion_Input
--IF EXISTS (SELECT name FROM sys.stats WHERE name = N'kl_x_contract_keyword_GH' AND object_id = OBJECT_ID(N'dbo.x_contract_keyword') ) DROP STATISTICS dbo.x_contract_keyword.kl_x_contract_keyword_GH		CREATE STATISTICS kl_x_contract_keyword_GH		ON x_contract_keyword (keyword_sid)
--CREATE NONCLUSTERED INDEX IX_x_contract_keyword_GH ON [dbo].[x_contract_keyword] ([keyword_sid]) INCLUDE ([contract_sid])
--IF EXISTS (SELECT name FROM sys.stats WHERE name = N'kl_c_udf_GH' AND object_id = OBJECT_ID(N'dbo.c_udf') ) DROP STATISTICS dbo.c_udf.kl_c_udf_GH		CREATE STATISTICS kl_c_udf_GH		ON c_udf (label_id)
--CREATE NONCLUSTERED INDEX IX_x_contract_rights_GH ON [dbo].[x_contract_rights] ([udkey_1_group_sid]) INCLUDE ([contract_sid])
--CREATE NONCLUSTERED INDEX IX_x_contract_rights_GH2 ON [dbo].[x_contract_rights] ([udkey_1_sid]) INCLUDE ([contract_sid])

	/**************************************************************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
	/****** PARAMETER ONE **** Get Contract Id ********************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
	
	

	----'_All','_Percentage_ of List Price','_Percentage_ of Revenue','_Percentage_ of Net Revenue'
	--DECLARE @Contract_ID nvarchar(60)					SET @Contract_ID				=	'All'--'3002702,4101082'--'3002702,DTTEST1, 4000040,3001977,3001528,4000008,4000009,821297,MCPS - UK VIDEO'
	--DECLARE @UDF_Name nvarchar(600)						SET @UDF_Name					=	'Initial Fees' --'_Percentage_ of List Price' -- _Product_Share_Next','_Artist_Share_Next _Statement_Schedule  _Buyout_Expiration_Date  '_Contributors' -- _Scope_Next_Rights  _Scope  _Recipient _Contributors  _Template_Group_Statements Sales Reduction Multiplier
	--DECLARE @Contributor_Name nvarchar(600)				SET @Contributor_Name			=	'All' -- SEEKING 
	--DECLARE @Contributor_ID nvarchar(600)				SET @Contributor_ID				=	'All' -- _Statement_Schedule  _Buyout_Expiration_Date  '_Contributors' -- _Scope_Next_Rights  _Scope  _Recipient _Contributors  _Template_Group_Statements Sales Reduction Multiplier
	--DECLARE @Product_ID nvarchar(30)					SET @Product_ID					=	'All' --'BBCDVD1509,ABGD003P,ABGD005D' --'AVDD848S,ABGD003P'
	--DECLARE @Keyword nvarchar(60)						SET @Keyword					=	'All'--'3002702,3002695,4000008,4000009,821297'
	--DECLARE @Payee nvarchar(60)							SET @Payee						=	'All' --BRUNS
	--DECLARE @Report_Folder	nvarchar(200)				SET @Report_Folder				=	'/CasparReportManager/CRA V6.2/06 - Contracts'
	
	DECLARE @Direct_Output_CheckBox bit					SET @Direct_Output_CheckBox		=	0
	DECLARE @Direct_Input_Inclusion nvarchar(30)		SET @Direct_Input_Inclusion		=	'No' --  No , Yes_Product_Scope, Yes_Contract, Yes_Contributor_ID
	DECLARE @Direct_Input_Exclusion nvarchar(30)		SET @Direct_Input_Exclusion		=	'No' --  No , Yes_Product_Scope, Yes_Contract, Yes_Contributor_ID
	
	-- select * from  #udf_Possible_To_Select where UDF_Name LIKE '%rev%'

	/**************************************************************************************************************************************************************************************/
	/********** Fixed Paramters ***********************************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/

	DECLARE @Warehoue_Supporting_Server_n_Database nvarchar(100)
	IF (@Report_Folder LIKE '%CRA V6.2%') BEGIN SET @Warehoue_Supporting_Server_n_Database = 'WGB01DB7047.Caspar_Warehouse_Supporting'  END 
	IF (@Report_Folder LIKE '%CRA 6.2 - Live - WGB01DB1041%') BEGIN SET @Warehoue_Supporting_Server_n_Database = 'WGB01DB1041.Caspar_Warehouse_Supporting'  END 
	

		SET @Contract_ID			= REPLACE(REPLACE(@Contract_ID,', ',',') ,' ,',',') 
		SET @Product_ID				= REPLACE(@Product_ID,' ','') 
		SET @Contributor_ID			= REPLACE(@Contributor_ID,' ','') 
	--	SET @Contributor_Name		= REPLACE(@Contributor_Name,' ','') 
		
		DECLARE @SQL nvarchar(max)
		DECLARE @UDF_Name_Orginal nvarchar(100) SET @UDF_Name_Orginal = @UDF_Name
		IF (@UDF_Name= '_All' AND @Contract_ID = 'All' AND @Contributor_Name = 'All'AND @Contributor_ID = 'All'AND @Product_ID = 'All'AND @Keyword = 'All'AND @Payee = 'All')		BEGIN SET @Contract_ID = '999'		SET  @UDF_Name = '_Template_Group_Statements'  END
	

	/**************************************************************************************************************************************************************************************/
	/********** Get Possible UDF Labels For Drop Down *************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
		
	IF OBJECT_ID('tempdb..#udf_Possible_To_Select') IS NOT NULL DROP TABLE #udf_Possible_To_Select
	CREATE TABLE #udf_Possible_To_Select (UDF_Name nvarchar(60) )
	INSERT INTO #udf_Possible_To_Select (UDF_Name)
	select DISTINCT REPLACE(REPLACE(label_id,'''',''),'`','') as UDF_Name from c_udf --where label_ID LIKE '%art%'
	where udf_sid IN (4,5,7,13,35,36,42,75,76,82,85,98,99,102,104,106,111,112,113,131,139,140,176,180,186,199,200,201,202,203,204,205,206,207,208,209,210,211,212,215,217,1968681879,1968681881,1968681882,1968681883,1968681885,1968681887,1968681890,1968681891,1968681892,1968681893,1968681894,1968681898,1968681907,1968681909,1968681910,1968681914,1968681915,1968681916,215,115,138,209,175,75,1968681908,201,181,109,1968681885,1968681879,146,152,1968681871,212,32,1968681891,35,106,1968681882,1968681888,135,112,98,113,184,1968681894,81,1968681906,64,1968681883,1968681914,207,1968681877,7,150,70,141,210,176,84,204,110,61,104,4,147,1968681874,211,188,1968681892,1968681898,36,205,1968681881,85,13,42,5,199,1968681869,99,139,128,76,202,108,82,182,88,1968681909,208,102,1968681915,1968681872,1968681873,140,186,1968681890,217,203,1968681887,1968681910,111,103,1968681916,114,1968681893,174,131,180,206,1968681907,1968681884,8,1968681870,151,200)

	UPDATE #udf_Possible_To_Select 
	SET UDF_Name = REPLACE(UDF_Name,'%','_Percentage_')
	from #udf_Possible_To_Select where LEFT(UDF_Name,1) = '%'

	INSERT INTO #udf_Possible_To_Select (UDF_Name)
				select '_Artist_Shares' as UDF_Name
	UNION ALL	select '_Artist_Share_Next' as UDF_Name
	UNION ALL	select '_Recipient' as UDF_Name
	UNION ALL	select '_Template_Group_Default' as UDF_Name
	UNION ALL	select '_Template_Group_Advance' as UDF_Name
	UNION ALL	select '_Template_Group_Accruals' as UDF_Name
	UNION ALL	select '_Template_Group_Accruals_39' as UDF_Name
	UNION ALL	select '_Template_Group_Statements' as UDF_Name
	UNION ALL	select '_All' as UDF_Name
	UNION ALL	select '_Scope' as UDF_Name
	UNION ALL	select '_Scope_Next_Rights' as UDF_Name
	UNION ALL	select '_Product_Share_Not_Available' as UDF_Name
	UNION ALL	select '_Product_Share_Next' as UDF_Name
	UNION ALL	select '_Status' as UDF_Name
	UNION ALL	select '_Buyout_Expiration_Date' as UDF_Name
	UNION ALL	select '_Statement_Schedule' as UDF_Name
	UNION ALL	select '_Section_Group' as UDF_Name
	UNION ALL	select '_Last_Approved_Statement' as UDF_Name
	
	/*	select UDF_Name	from #udf_Possible_To_Select ORDER BY CASE WHEN UDF_Name LIKE '_All' THEN '0' WHEN LEFT(UDF_Name,1) = '_' THEN '1'	 ELSE UDF_Name END ASC */
	
	IF (@Payee						&lt;&gt;		'All')													BEGIN SET @UDF_Name_Orginal = 'Payee' SET @UDF_Name = 'Payee'  END 
	IF (@UDF_Name_Orginal			IN		('_Buyout_Expiration_Date'))							BEGIN SET @UDF_Name = 'Buyout Term,TV First Transmission'			SET @UDF_Name = REPLACE(@UDF_Name,',',''',''')	SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')'  END 
	IF (@UDF_Name_Orginal			IN		('_Artist_Share_Next','_Contributors','_Artist_Shares'))BEGIN SET @UDF_Name = 'Artists Share,Equity/MU/PACT Contrib Fees'	SET @UDF_Name = REPLACE(@UDF_Name,',',''',''')	SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')'  END
	IF (@UDF_Name_Orginal			LIKE	'_Recipient')											BEGIN SET @UDF_Name = 'Payment Recipient Export ID'					SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')' END
	IF (LEFT(@UDF_Name_Orginal,9)	LIKE	'_Template')											BEGIN 																SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')' END
	IF (@UDF_Name_Orginal			IN		('_Scope','_Scope_Next_Rights'))						BEGIN 																SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')' END
	IF (@UDF_Name_Orginal			LIKE	('_Status'))											BEGIN SET @UDF_Name = 'Contribution Type'							SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')' END
	IF (@UDF_Name_Orginal			LIKE	'_Percentage_ of List Price')							BEGIN 																SET @UDF_Name = 'where label_id LIKE ''%List Price%''' END 
	IF (@UDF_Name_Orginal			LIKE	'_Percentage_ of Revenue')								BEGIN 																SET @UDF_Name = 'where label_id LIKE ''%of revenue%''' END 
	IF (@UDF_Name_Orginal			LIKE	'_Percentage_ of Net Revenue')							BEGIN 																SET @UDF_Name = 'where label_id LIKE ''%of Net Revenue%''' END 
	IF (@UDF_Name_Orginal			LIKE	'_All')													BEGIN SET @UDF_Name = '_All' END
	IF (@UDF_Name_Orginal			IN		('_Statement_Schedule','_Section_Group'))				BEGIN SET @UDF_Name = 'Contribution Type'							SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')' END
	IF (@UDF_Name_Orginal			IN		('_Product_Share_Next'))								BEGIN SET @UDF_Name = 'Product Share Percentage'					SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')' END
	IF (@UDF_Name_Orginal			LIKE	'_Artist_Share_Next')									BEGIN SET @UDF_Name = 'Artists Share,Equity/MU/PACT Contrib Fees'	SET @UDF_Name = REPLACE(@UDF_Name,',',''',''')	SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')'  END
	IF (LEFT(@UDF_Name_Orginal,1)	 &lt;&gt;		'_')													BEGIN SET @UDF_Name  = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')'  END 
	IF (@UDF_Name_Orginal			IN		('_Product_Share_Not_Available'))						BEGIN 																SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  (''G Was Here'')' END
	IF (@UDF_Name_Orginal			LIKE	'_Last_Approved_Statement')								BEGIN SET @UDF_Name = 'None'										SET @UDF_Name = 'where REPLACE(label_id,'''''''','''') IN  ('''+@UDF_Name+''')' END
	
	
	
