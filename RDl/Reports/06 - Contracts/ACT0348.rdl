			-- select c.Contributor_ID, c.user_contact_sid,  c.Multi_Particpant,  c.Contributor_Name, xcu.*
			from #x_contract_udf xcu
			INNER JOIN c_contact c on c.contact_sid = xcu.contact_sid
			where xcu.udf_sid IN (206,141,209,184,217,151,212,8,207,146,211)
			END
	  	   
		  IF (@UDF_Name_Orginal IN ('_All','_Recipient'))
		  BEGIN
				IF OBJECT_ID('tempdb..#Multi_Contributor_ID') IS NOT NULL DROP TABLE #Multi_Contributor_ID
				;WITH CTE_Contrib AS
				(
				select '_Recipient' as UDF_Descr, xc.contract_id, xcul.contract_sid, value, ISNULL(user_contact_sid,xcp.contact_sid) as user_contact_sid, xcul.sort_order_nbr, xcul.seq_nbr, xc.Multi_Particpant,udf_sid
				from			x_contract_udf_lookup xcul
				INNER JOIN		#x_contract xc on xc.contract_sid = xcul.contract_sid
				LEFT JOIN		x_contract_participant			xcp with (nolock) on xcul.contract_sid = xcp.contract_sid AND xcul.sort_order_nbr = xcp.sort_order_nbr
				where  xc.Multi_Particpant = 1 and udf_sid = 215  --215
				)
				select xc.UDF_Descr, xc.contract_id, xc.contract_sid, value, xc.user_contact_sid, xc.sort_order_nbr, xc.seq_nbr, xc.Multi_Particpant, cc.contact_id AS Contributor_ID, LEFT(cc.formatted_name,30) AS Contributor_Name
				INTO #Multi_Contributor_ID
				from CTE_Contrib xc
				INNER JOIN	c_contact  cc on cc.contact_sid = xc.user_contact_sid	

				UPDATE #x_contract_udf
				SET Contributor_ID = c.Contributor_ID, user_contact_sid = c.user_contact_sid, Contributor_Name = c.Contributor_Name
				-- select c.Contributor_ID, c.user_contact_sid,  c.Multi_Particpant,  c.Contributor_Name, xcu.*
				from			#x_contract_udf xcu
