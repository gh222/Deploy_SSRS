		/*****************************************************************************************************/
		
			DECLARE @Keyword_Filter nvarchar(200) SET @Keyword_Filter = ''
			IF (@Keyword &lt;&gt; 'All')
			BEGIN
			IF OBJECT_ID('tempdb..#x_keyword') IS NOT NULL DROP TABLE #x_keyword
			
			DECLARE @Keyword_sid int SET @Keyword_sid = (select keyword_sid 	from c_keyword where descr =  @Keyword)
			
			select contract_sid
			INTO #x_keyword
			from			x_contract_keyword xck
			where keyword_sid = @Keyword_sid
		
			SET @Keyword_Filter = 'INNER JOIN #x_keyword k on k.contract_sid = xc.contract_sid '
			END

		
		/*****************************************************************************************************/
		/****************************** Product Filter *******************************************************/
		/*****************************************************************************************************/
	

		IF OBJECT_ID('tempdb..#Product_Filter') IS NOT NULL DROP TABLE #Product_Filter
		IF OBJECT_ID('tempdb..#x_contract_Product_filter') IS NOT NULL DROP TABLE #x_contract_Product_filter  
		CREATE TABLE #Product_Filter			(udkey_1_sid int) 
		CREATE TABLE #x_contract_Product_filter (contract_sid int,udkey_1_sid int,udkey_1_group_sid int)
		DECLARE @Product_Filter nvarchar(max)		SET @Product_Filter = ''
		DECLARE @Product_Filter_For_Contracts nvarchar(max)		SET @Product_Filter_For_Contracts = ''
		
		IF (@Product_ID &lt;&gt; 'All')								BEGIN	SET @Product_Filter = 'where udkey_1_id IN ('''+REPLACE(@Product_ID,',',''',''')+''')' SET @Product_Filter_For_Contracts = 'INNER JOIN #x_contract_Product_filter xcpf on xcpf.contract_sid = xc.contract_sid' END
		IF (@Direct_Input_Inclusion = 'Yes_Product_Scope' ) 	BEGIN   SET @Product_Filter ='where cu1.udkey_1_sid in (select distinct udkey_1_sid	from   #x_contract_Inclusion    xci	INNER JOIN c_udkey_1 cu1 on cu1.udkey_1_id = xci.Inclusion_Column)'			SET @Product_ID = 'Not_All'	END
		IF (@Product_ID = 'All')								BEGIN	SET @Product_Filter = ''  END
		IF (@Direct_Input_Exclusion = 'Yes_Product_Scope' )		BEGIN	SET @Product_Filter = 'where cu1.udkey_1_id NOT IN (select distinct exclusion_column from #x_contract_Exclusion)'   END 
					

		SET @SQL =
				'
				INSERT INTO #Product_Filter (udkey_1_sid) 
				select cu1.udkey_1_sid  
				from dbo.c_udkey_1 cu1
				'+@Product_Filter+'
				'
				exec  (@SQL)
			
		--IF NOT EXISTS (SELECT name FROM tempdb.sys.indexes WHERE name LIKE N'IX_Product_Filter_GH' AND object_id = OBJECT_ID(N'tempdb..#Product_Filter') ) BEGIN CREATE NONCLUSTERED INDEX IX_Product_Filter_GH ON #Product_Filter (udkey_1_sid)  END
													
		SET @SQL = 
				'
				INSERT INTO #x_contract_Product_filter (contract_sid,udkey_1_sid,udkey_1_group_sid )
				select  xcr.contract_sid, xcr.udkey_1_sid, null as udkey_1_group_sid
				from dbo.X_contract_rights xcr
				INNER JOIN #Product_Filter pf on pf.udkey_1_sid = xcr.udkey_1_sid
				where xcr.udkey_1_sid IS NOT NULL 
				UNION
				select  xcr.contract_sid, cu1d.udkey_1_sid, xcr.udkey_1_group_sid
				from dbo.X_contract_rights xcr
				INNER JOIN dbo.c_udkey_1_group_dtl cu1d on cu1d.udkey_1_group_sid = xcr.udkey_1_group_sid
				INNER JOIN #Product_Filter pf on pf.udkey_1_sid = cu1d.udkey_1_sid
				where xcr.udkey_1_group_sid IS NOT NULL 
				'
				exec (@SQL)
		



	/**************************************************************************************************************************************************************************************/
	/*********************** Contract Filter *********************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
		
	IF OBJECT_ID('tempdb..#x_contract') IS NOT NULL DROP TABLE #x_contract
	CREATE TABLE #x_contract (contract_sid int, Payee_contact_sid int, Contract_id nvarchar(30),Contract_Export_ID nvarchar(30),Contract_Description nvarchar(60), Royalty_period nvarchar(30),	Admin_class nvarchar(30),		Contract_Status nvarchar(30),		Account nvarchar(30),		x_O nvarchar(1),		Contributor_ID nvarchar(30),		Contributor_Payee_ID nvarchar(30),		Contributor_Statement_ID_n_Slot nvarchar(30),		Contributor_Order nvarchar(30),		Contributor_Description nvarchar(30),		Contributor_Payee_Description nvarchar(30),		Contributor_Statement_Description nvarchar(30),		x_1 nvarchar(1),		Preparer nvarchar(30),		Keyword nvarchar(30),		x_2 nvarchar(1),		Date_Report_Extracted date,		Date_Royalty_Period_End date,		Date_Due date,		Date_Run date,		Date_Approved date,		Approval_comment nvarchar(60),		x_3 nvarchar(1),		Units decimal(10,2),		Amount decimal(10,2),		x_4 nvarchar(1),		Sales_Period nvarchar(30),		Currency nvarchar(30),		Product_ID nvarchar(30),		Sales_Type_ID nvarchar(30),		Distrubtor nvarchar(30),		Department nvarchar(30),		Media nvarchar(30),		SAP_Territory nvarchar(30),		Contribution_Type nvarchar(30),		x_5 nvarchar(1)	,deal_sid int, royalty_period_sid int, preparer_sid int,admin_class_sid int, Contributor_contact_sid INT,Statement_contact_sid INT, multi_Particpant bit,Statement_Contributor_ID_n_Slot nvarchar(30), Statement_Contributor_ID_Description nvarchar(30), recipient_group_sid nvarchar(30), status_sid int, TransType nvarchar(30), Payee_ID nvarchar(30), Payee_ID_Name nvarchar(30), Sort_order_nbr int, Contributor_ID_Name nvarchar(30), Date_royalty_Period nvarchar(30)	, Lotscaps nvarchar(30))
	DECLARE @Contract_Filter nvarchar(100) 

	IF (@Contract_ID &lt;&gt; 'All') BEGIN 	SET @Contract_Filter = 'AND xc.contract_id IN ('''+REPLACE(@Contract_ID,',',''',''')+''')' END
	IF (@Contract_ID = 'All') BEGIN 	SET @Contract_Filter = '' END

		SET @SQL =
		'
		INSERT INTO #x_contract (contract_sid,contract_id, Contract_Description,status_sid, admin_class_sid,Multi_Particpant) 
		select xc.contract_sid, LEFT(xc.contract_id,30) AS contract_id, LEFT(REPLACE(xc.descr,'','',''''),60) as Contract_Description,xc.status_sid, xc.admin_class_sid, (CASE WHEN admin_class_sid IN (13,23) THEn 1 ELSE 0 END) AS Multi_Particpant
		from x_contract xc
		'+@Contact_Filter+'
		'+@Keyword_Filter+'
		'+@Direct_Input_Inclusion_Contract_Filter+'
		'+@Direct_Input_Inclusion_Contributor_Filter+'
		'+@Direct_Input_Inclusion_Product_Filter_On_Contract+'
		'+@Product_Filter_For_Contracts+'
		where xc.original_contract_sid = xc.contract_sid AND xc.admin_class_sid NOT IN (1,7) AND xc.status_sid &lt;&gt; 18
		'+@Contract_Filter+'
		'+@Direct_Input_Exclusion_Contract_Filter+'
		'
		exec  (@SQL)
		
	
