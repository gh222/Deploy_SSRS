		--print (@SQL)


	/**************************************************************************************************************************************************************************************/
	/*********************** Contributor Filter *******************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
	
	
			IF OBJECT_ID('tempdb..#C_Contact') IS NOT NULL DROP TABLE #C_Contact
			CREATE TABLE #C_Contact (contact_sid int, Contributor_ID nvarchar(30), Contributor_Name nvarchar(30),contract_sid int)

			DECLARE @Contact nvarchar(100) SET @Contact =''
			DECLARE @Contact_Filter nvarchar(100) SET @Contact_Filter = ''

			IF (@Contributor_Name &lt;&gt; 'All' AND @Direct_Input_Inclusion = 'No')								BEGIN SET @Contact_Filter = 'where cc.formatted_name LIKE ''%'+REPLACE(@Contributor_Name,',',''',''')+'%'''  SET @Contributor_ID = ''   END
			IF (@Contributor_Name = 'All'  AND @Contributor_ID &lt;&gt; 'All' AND @Direct_Input_Inclusion = 'No')	BEGIN SET @Contact_Filter  = 'where cc.contact_id IN ('''+REPLACE(@Contributor_ID,',',''',''')+''')'  SET @Contributor_Name = '' END

			SET @SQL =
			'
			INSERT INTO #C_Contact (contact_sid, Contributor_ID, Contributor_Name,contract_sid)
			select cc.contact_sid, contact_ID AS Contributor_ID, LEFT(Formatted_name,30) as Contributor_Name, xcp.contract_sid
			from c_contact cc
			INNER JOIN x_contract_participant xcp on xcp.contact_sid = cc.contact_sid
			'+@Contact_Filter+'
			'+@Direct_Input_Inclusion_Contributor_Filter+'
			'+@Direct_Input_Exclusion_Contributor_Filter+'
			'
			exec  (@SQL)
			
			IF (@Contributor_Name = 'All'  AND @Contributor_ID = 'All')		BEGIN SET @Contact_Filter = '' END
			IF (@Contributor_Name &lt;&gt; 'All' )								BEGIN SET @Contact_Filter = ' INNER JOIN #C_Contact cc on cc.contract_sid = xc.contract_sid ' END
			IF (@Contributor_ID &lt;&gt; 'All')									BEGIN SET @Contact_Filter = ' INNER JOIN #C_Contact cc on cc.contract_sid = xc.contract_sid ' END


			SET @Direct_Input_Inclusion_Contributor_Filter = ''
			IF (@Direct_Input_Inclusion		= 'Yes_Contributor_ID') BEGIN SET @Direct_Input_Inclusion_Contributor_Filter = ' INNER JOIN #C_Contact xci on xci.contract_sid = xc.contract_sid '   END
	
	/**************************************************************************************************************************************************************************************/
	/*********************** Payee Filter *******************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
	
		DECLARE @Payee_Filter_Drop_Down nvarchar(100) SET @Payee_Filter_Drop_Down = ''
		DECLARE @Payee_Filter_Default nvarchar(100) SET @Payee_Filter_Default = ''
		IF ( @Payee &lt;&gt; 'All')
		BEGIN
		
				IF OBJECT_ID('tempdb..#Payee') IS NOT NULL DROP TABLE #Payee
				CREATE TABLE #Payee (contact_sid int,formatted_name nvarchar(30) )
				SET @SQL =
				'
				INSERT INTO #Payee (formatted_name )
				select distinct left(formatted_name,30) as formatted_name  from c_contact where formatted_name LIKE ''%'+@Payee+'%''
				'
				exec (@SQL)
				SET @Payee_Filter_Drop_Down = ' INNER JOIN #Payee p on p.formatted_name = xcu.value '
				SET @Payee_Filter_Default = ' INNER JOIN #Payee p on p.formatted_name = xcu.udf_value '
		END

