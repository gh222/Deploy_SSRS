      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>;with CTE AS
(

select  descr as keyword_description from c_keyword where descr &lt;&gt; ' '
UNION ALL
select  'All' keyword_description
)
select *
from CTE
Order by 
(CASE WHEN keyword_description = 'All' THEN 1 ELSE 2 END)</CommandText>
      </Query>
