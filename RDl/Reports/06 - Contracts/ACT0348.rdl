			--------		WHERE Roww &lt;&gt; 1
			--------END
	
			IF (@Direct_Input_Inclusion	IN	('Yes_Contributor_ID'))								BEGIN DELETE from #x_contract_udf  where user_contact_sid NOT IN (select distinct contact_sid from #c_contact) and Multi_Particpant = 1 END
			IF (@UDF_Name_Orginal IN		('_Contributors', '_Recipient','_Artist_Shares'))	BEGIN DELETE #x_contract_udf where Contributor_ID IS NULL and Multi_Particpant = 1 END --UPDATE #x_contract_udf SET udkey_1_sid = NULL, udkey_1_group_sid = null END
			IF (@UDF_Name_Orginal NOT IN	('_Contributors', '_Recipient','_Artist_Shares'))	BEGIN UPDATE #x_contract_udf SET Contributor_ID = 'Many', Contributor_Name = 'Many' from #x_contract_udf where Contributor_ID is null END
		
