				LEFT JOIN		x_contract_participant			xcp with (nolock) on xcul.contract_sid = xcp.contract_sid AND xcul.sort_order_nbr = xcp.sort_order_nbr
				where  xc.Multi_Particpant = 1 and udf_sid = 202  --215
