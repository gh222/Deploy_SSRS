  <DataSources>
    <DataSource Name="Warehouse_Supporting">
      <ConnectionProperties>
        <DataProvider>SQL</DataProvider>
        <ConnectString>Data Source=WGB01DB7049;Initial Catalog=Caspar_Warehouse_Supporting</ConnectString>
        <Prompt>Specify a user name and password for data source Warehouse_Supporting:</Prompt>
      </ConnectionProperties>
      <rd:SecurityType>DataBase</rd:SecurityType>
      <rd:DataSourceID>5a8203b1-4de3-4c34-90d9-984be26332c1</rd:DataSourceID>
    </DataSource>
  </DataSources>
  <DataSets>
    <DataSet Name="Warehouse">
      <Query>
        <DataSourceName>Warehouse_Supporting</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Caspar_Layer_Name_Input">
            <Value>=Parameters!Caspar_Layer_Name_Input.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

		/********************************* Update SSIS Configurations Table *********************************************************************************/
		DECLARE @Caspar_Layer_Name nvarchar(60) SET @Caspar_Layer_Name =  @Caspar_Layer_Name_Input --'IPA WGB01DB7042 UAT' 'IPA WGB01DB1002 LIVE'
		UPDATE [SSIS].[Configurations] SET Turn_On = 0 FROM [SSIS].[Configurations] where Turn_On = 1
		UPDATE [SSIS].[Configurations] SET Turn_On = 1 FROM [SSIS].[Configurations] where Caspar_Layer_Name = @Caspar_Layer_Name
		
		--select * from [SSIS].[Configurations]
		
		/********************************* Run Agent ******************************************************************************************************/
		DECLARE @return int
		DECLARE @packagename nvarchar(100) SET @packagename = 'Caspar_Warehouse_IPA_Transformer';
		EXECUTE @return = msdb.dbo.sp_start_job @packagename
		WAITFOR DELAY '00:00:02';


		/********************************* Only output results once Package has finished running ***********************************************************/
		DECLARE @Start datetime	SET @Start = GETDATE()
		WHILE (SELECT MAX(execution_ID) FROM SSISDB.catalog.executions where package_name LIKE 'IPA_Transformer.dtsx' and end_time IS NULL) &gt; 0
		BEGIN
			IF (SELECT MAX(execution_ID) FROM SSISDB.catalog.executions where package_name LIKE 'IPA_Transformer.dtsx' and end_time IS NULL) IS NULL
					BREAK
			IF(DATEDIFF(s, @Start, GETDATE())) &gt;60
				BREAK
			ELSE
				CONTINUE
		END
		--PRINT('FINISHED')

		;WITH CTE AS
		(
		select 'IPA_Transformer' AS Package_Type, 'Job Complete' AS [State], '\\Worldwide\dfs\WDL\Departmental Secure\TA\Caspar_Extracts\IPA' AS OutPut_Location, CAST(start_time as datetime) as start_time, CAST(end_time as datetime) as end_time, package_name, ROW_NUMBER() OVER (PARTITION BY package_name order by end_time desc) AS Roww
		from SSISDB.catalog.executions 
		where package_name LIKE 'IPA_Transformer.dtsx'
		)
