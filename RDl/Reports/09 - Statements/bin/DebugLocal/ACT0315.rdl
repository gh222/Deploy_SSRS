  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>DECLARE @PPOPactAgreementFlagSid sid
SELECT @PPOPactAgreementFlagSid = udf_sid FROM c_udf WHERE name = 'PPOPactAgreementFlag'

SELECT contract_id,product_id FROM(
	SELECT contract_id
		,product_id
		,CASE WHEN LookupValue IS NULL THEN 
				DefaultValue 
			ELSE 
				LookupValue 
			END as PPOPactAgreementFlag
	FROM(
		SELECT xc.contract_id 
			,(SELECT CASE udf_value WHEN 18 THEN 'YES' ELSE 'NO' END FROM x_contract_udf xcu WHERE xcu.contract_sid = xc.contract_sid AND xcu.udf_sid = @PPOPactAgreementFlagSid) as DefaultValue
			,(SELECT TOP 1 CASE value WHEN 18 THEN 'YES' ELSE 'NO' END 
				FROM x_contract_udf_lookup xcul 
				LEFT JOIN x_period xp ON xcul.start_period_sid = xp.period_sid
				WHERE xcul.contract_sid = xc.contract_sid AND xcul.udf_sid = @PPOPactAgreementFlagSid 
				AND (xp.period_ending_date &lt; GETDATE() OR xp.period_ending_date IS NULL)
				ORDER BY sort_order_nbr DESC) as LookupValue
			,cu1.udkey_1_id as Product_id
		FROM x_contract xc
		INNER JOIN(SELECT contract_sid,udkey_1_sid
					FROM x_contract_rights xcr
					WHERE xcr.udkey_1_group_sid is null 
				UNION
					SELECT contract_sid,cu1dtl.udkey_1_sid
					FROM x_contract_rights xcr
					INNER JOIN c_udkey_1_group_hdr cu1gh ON cu1gh.udkey_1_group_sid = xcr.udkey_1_group_sid
					INNER JOIN c_udkey_1_group_dtl cu1dtl ON  cu1dtl.udkey_1_group_sid = xcr.udkey_1_group_sid
				)products ON products.contract_sid = xc.contract_sid 
		INNER JOIN c_udkey_1 cu1 ON products.udkey_1_sid = cu1.udkey_1_sid
		WHERE xc.contract_sid = xc.original_contract_sid
	)a
)b
WHERE PPOPactAgreementFlag = 'YES'
--AND contract_id = 'FPI013914 LEFT BANK'
ORDER BY contract_id ,product_id
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
