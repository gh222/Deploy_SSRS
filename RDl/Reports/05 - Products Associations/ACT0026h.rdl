  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>DECLARE	@keyword varchar(255)

if @keyword is null set @keyword = '%'
if @keyword ='' set @keyword = '%'



IF OBJECT_ID('tempdb..#contracts') IS NOT NULL DROP TABLE #contracts
IF OBJECT_ID('tempdb..#contractproducts') IS NOT NULL DROP TABLE #contractproducts
IF OBJECT_ID('tempdb..#results') IS NOT NULL DROP TABLE #results

create table #contracts(
	contract_sid sid
)


insert into #contracts
select xc.contract_sid
FROM dbo.x_contract xc
JOIN dbo.c_status s ON xc.status_sid = s.status_sid
WHERE s.status_id IN ('ACTIVE','INSETUP','INREVISION','COMPLETE','APPROVED')
AND xc.contract_sid = xc.original_contract_sid


create table #contractproducts(
	contract_sid id,
	product_sid  sid null,
	product_id  id null
)

insert into #contractproducts
select 	distinct 
	xc.contract_sid, 
	cu1.udkey_1_sid,
	cu1.udkey_1_id
FROM #contracts xc
INNER JOIN x_contract_rights xcr ON xc.contract_sid = xcr.contract_sid
LEFT JOIN c_udkey_1 cu1 ON cu1.udkey_1_sid = xcr.udkey_1_sid 
LEFT JOIN c_udkey_1_group_hdr cu1gh ON cu1gh.udkey_1_group_sid = xcr.udkey_1_group_sid
WHERE xcr.udkey_1_group_sid is null  --&lt;&lt; PRODUCTS

--now update product lists
insert into #contractproducts
select 	distinct 
	xc.contract_sid, 
	cu1.udkey_1_sid,
	cu1.udkey_1_id
from 	#contracts xc ,x_contract_rights xcr, c_udkey_1 cu1,  c_udkey_1_group_hdr cu1gh, c_udkey_1_group_dtl cu1dtl
 where  	xcr.udkey_1_group_sid is not null --&lt;&lt; PRODUCTS FROM PRODUCT LISTS
	and xc.contract_sid = xcr.contract_sid
	and cu1gh.udkey_1_group_sid = xcr.udkey_1_group_sid
	and cu1dtl.udkey_1_group_sid = xcr.udkey_1_group_sid
	and cu1.udkey_1_sid = cu1dtl.udkey_1_sid

create table #results(
	product_sid  id null,
	product_id  id null,
	descr descr null,
	release_date varchar(255) null
)

insert into #results
select c.udkey_1_sid, c.udkey_1_id, c.descr , null 
from c_udkey_1 c left outer join #contractproducts cp
on c.udkey_1_sid = cp.product_sid
where cp.product_id is null

update #results set release_date =  cu1u_rd.udf_value 
from #results inner join c_udkey_1_udf cu1u_rd on cu1u_rd.udkey_1_sid = #results.product_sid
where cu1u_rd.udf_sid = 20 

-- Show products that are not on any contracts
select #results.product_id as 'Product ID', #results.descr as 'Description', #results.release_date as 'Release Date'
from #results 
	left join c_udkey_1_keyword ON #results.product_sid = c_udkey_1_keyword.udkey_1_sid
	left join c_keyword ON c_udkey_1_keyword.keyword_sid = c_keyword.keyword_sid
where (c_keyword.descr &lt;&gt; @keyword or c_keyword.descr is null)
group by #results.product_id, #results.descr ,#results.release_date
order by product_id
</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Product_ID">
          <DataField>Product ID</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Description">
          <DataField>Description</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Release_Date">
          <DataField>Release Date</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Keyword">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>SELECT     
descr as label
, descr as value
FROM         c_keyword 
UNION SELECT  '&lt;Blank&gt;' AS label,
 '' AS value ORDER BY descr</CommandText>
      </Query>
      <Fields>
        <Field Name="label">
          <DataField>label</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="value">
          <DataField>value</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
  </DataSets>
  <ReportParameters>
    <ReportParameter Name="keyword">
      <DataType>String</DataType>
      <AllowBlank>true</AllowBlank>
      <Prompt>Keyword for Exclusion (&lt;blank&gt; for all)</Prompt>
      <ValidValues>
        <DataSetReference>
          <DataSetName>Keyword</DataSetName>
          <ValueField>value</ValueField>
          <LabelField>label</LabelField>
        </DataSetReference>
      </ValidValues>
