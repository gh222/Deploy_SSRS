		--DECLARE @Contract_ID nvarchar(60)					SET @Contract_ID				= '4000409,3002702'--'4000008,3002702' --'3002702,3002695,4000008,4000009,821297'
		--DECLARE @Product_ID nvarchar(30)					SET @Product_ID					= 'All'--'ABHH916E' --'BBCDVD1509,ABGD003P,ABGD005D' --'AVDD848S,ABGD003P'
