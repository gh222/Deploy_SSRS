					select ''Default'' AS UDF_Type, xc.Contract_id as Contract_ID ,xc.Contract_id as Contract_ID_Export,	xcp.contact_sid  as contact_sid, 1 as seq_nbr, 1 as sort_order_nbr,  ''N'' Multi_Particpant	,NULL AS udkey_1_sid, NULL AS udkey_1_group_sid, xcu.contract_sid	, CAST(xcu.udf_value as nvarchar(10)) AS Artist_Share	,xc.Contract_Status,xc.Admin_class, xc.Contribution_Type,xc.PG,LEFT(REPLACE(xc.contract_description,'','',''''),30) as contract_description, xc.keyword
					from			dbo.x_contract_udf		xcu
						INNER JOIn		#x_contract						xc on xc.contract_sid = xcu.contract_sid
						INNER JOIn		dbo.x_contract_participant			xcp on xc.contract_sid = xcp.contract_sid AND xcp.contact_sid = xcp.contact_sid
