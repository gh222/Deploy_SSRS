					from			CTE_Artist_Shares  cte
					INNER JOIN		dbo.c_contact			cc on cc.contact_sid = cte.contact_sid
					)
					INSERT INTO #Artist_Shares ([UDF_Type]      ,[PG]      ,[Contract_ID]      ,[Contract_ID_export]      ,[Artist_Seq]      ,[Artist_Sort]      ,[Multi_Particpant]      ,[Artist_Share]     ,[Contributor_ID]      ,[udkey_1_sid]      ,[udkey_1_group_sid]      ,[contract_sid]      ,[contact_sid]      ,[Contract_Status]      ,[Admin_class]      ,[Contributor_Name]      ,[Contribution_Type] , True_Artist_Share_Percent, contract_description,keyword)
					select cte.UDF_Type, cte.PG, cte.Contract_ID, cte.Contract_ID_export, cte.[Artist_Seq], cte.[Artist_Sort], cte.Multi_Particpant,cte.Artist_Share,cte.Contributor_ID, cu1gd.udkey_1_sid, cte.udkey_1_group_sid, cte.contract_sid,cte.contact_sid	,cte.Contract_Status,cte.Admin_class , cte.contributor_Name, cte.Contribution_Type , cte.True_Artist_Share_Percent, cte.contract_description, cte.keyword
					from CTE_Gather_Contact_Details cte
					LEFT JOIN c_udkey_1_group_dtl cu1gd on cu1gd.udkey_1_group_sid = cte.udkey_1_group_sid
									
					'
					EXEC (@SQL)
					
			END
				
		

