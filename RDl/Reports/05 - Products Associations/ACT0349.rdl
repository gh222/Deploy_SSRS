								LEFT JOIN		dbo.c_udkey_1				cu1 on cu1.udkey_1_sid = xcu.udkey_1_sid
								LEFT JOIN		dbo.c_udkey_1_group_dtl		cu1_2 on cu1_2.udkey_1_group_sid = xcu.udkey_1_group_sid
