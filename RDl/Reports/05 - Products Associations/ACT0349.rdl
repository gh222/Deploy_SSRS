		IF OBJECT_ID('tempdb..#Attached_Products') IS NOT NULL DROP TABLE #Attached_Products
		CREATE TABLE #Attached_Products (contract_sid int,  udkey_1_sid int, udkey_1_group_sid int, Product_ID nvarchar(30), Product_Description nvarchar(60),  Default_Type bit, Product_Share	decimal(10,2), Product_List nvarchar(60), seq_nbr int,sort_order_nbr int, Duplicate_PS bit, UDF_Type nvarchar(10) DEFAULT 'Default')
		
		SET @SQL =
		'
				;WITH CTE_Attached_Products AS
				(
					'+@Product_Filter+'
				)
				INSERT INTO #Attached_Products (contract_sid ,  udkey_1_sid , udkey_1_group_sid , Product_ID , Product_Description ,  Default_Type , Product_Share	, Product_List , seq_nbr ,sort_order_nbr , Duplicate_PS )
				select  distinct xcu.contract_sid,  xcu.udkey_1_sid, xcu.udkey_1_group_sid, LEFT(cu1.udkey_1_id,30) as Product_ID, REPLACE(LEFT(cu1.descr,60),'','','''') as Product_Description, NULL AS Default_Type, NULL AS Product_Share	, REPLACE(cu1_h.descr,'','','''') AS Product_List, 0 as seq_nbr,0 as  sort_order_nbr, 0 as Duplicate_PS
				from CTE_Attached_Products xcu
				LEFT JOIN		dbo.c_udkey_1				cu1 on cu1.udkey_1_sid = xcu.udkey_1_sid
				LEFT JOIN		dbo.c_udkey_1_group_hdr		cu1_h on cu1_h.udkey_1_group_sid = xcu.udkey_1_group_sid
