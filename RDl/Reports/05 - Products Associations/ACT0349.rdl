				INNER JOIN dbo.c_udkey_1_group_dtl cu1d on cu1d.udkey_1_group_sid = xcr.udkey_1_group_sid
				INNER JOIN c_udkey_1 cu1				on cu1.udkey_1_sid = cu1d.udkey_1_sid
				INNER JOIN #x_contract_Inclusion pf		on pf.Inclusion_Column = cu1.udkey_1_id
		
