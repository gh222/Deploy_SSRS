		INSERT INTO #C_Contact (contact_sid, Contributor_ID, Contributor_Name,contract_sid,recipient_group_sid)
		select  cc.contact_sid, contact_ID AS Contributor_ID, LEFT(Formatted_name,30) as Contributor_Name, xcp.contract_sid,recipient_group_sid
