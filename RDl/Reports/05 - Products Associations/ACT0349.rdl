					'
					;WITH CTE_Artist_Shares AS
					(
					select ''Lookup'' AS UDF_Type, xc.Contract_id as Contract_ID 	,CASE 
						WHEN xcu.user_contact_sid IS NOT NULL AND LEN(xcu.seq_nbr)  = 1 THEN xc.Contract_id + ''00'' + CAST(xcu.seq_nbr as nvarchar(3))
						WHEN xcu.user_contact_sid IS NOT NULL AND LEN(xcu.seq_nbr)  = 2 THEN xc.Contract_id  + ''0'' + CAST(xcu.seq_nbr as nvarchar(3))
						WHEN xcu.user_contact_sid IS NOT NULL AND LEN(xcu.seq_nbr)  = 3 THEN xc.Contract_id  + '''' + CAST(xcu.seq_nbr as nvarchar(3))
					 ELSE  xc.Contract_id END AS Contract_ID_Export,		
					ISNULL(xcu.user_contact_sid,xcp.contact_sid ) as contact_sid, xcu.seq_nbr, xcu.sort_order_nbr, xc.Multi_Particpant	,xcu.udkey_1_sid, xcu.udkey_1_group_sid, xcu.contract_sid	,CAST(xcu.value as nvarchar(10)) AS Artist_Share	,xc.Contract_Status,xc.Admin_class, xc.Contribution_Type,xc.PG, LEFT(REPLACE(xc.contract_description,'','',''''),30) as contract_description, xc.keyword
					from			dbo.x_contract_udf_lookup		xcu
