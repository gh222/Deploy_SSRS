			--select a.*, p.Product_Share
			UPDATE #Attached_Products
			SET Product_Share = CAST(CAST(p.Product_Share as float) as decimal(18,2)), Default_Type = 0
			--select	a.*, a.Product_Share, p.Product_Share
			from			#Attached_Products	a
			INNER JOIN		#Product_Share		p on a.contract_sid = p.contract_sid AND a.udkey_1_sid = p.udkey_1_sid 
			where a.Product_Share IS  NULL

			UPDATE #Attached_Products
			SET Product_Share = CAST(CAST(p.Product_Share as float) as decimal(18,2)) , Default_Type = 1
			--select a.*, p.Product_Share
			from			#Attached_Products	a
			LEFT JOIN		#Product_Share		p on a.contract_sid = p.contract_sid --AND a.udkey_1_sid &lt;&gt; p.udkey_1_sid
			where a.Default_Type IS NULL		and p.Sort_order_nbr = 0	AND a.Product_Share IS NULL

			SET @SQL =
			'
			UPDATE #x_contract
			SET Contract_Status = cs.status_id
			from			#x_contract xc
			LEFT JOIN		dbo.c_status	cs on cs.status_sid = xc.status_sid

			UPDATE #x_contract
			SET  Admin_class = xa.admin_class_id
			from #x_contract xc
			LEFT JOIN	dbo.x_admin_class xa   on xa.admin_class_sid = xc.admin_class_sid 

			UPDATE #x_contract
			SET  Contribution_Type = cu8.udkey_8_id
			--select xcu.udf_value, cu8.udkey_8_id
			from #x_contract xc
			LEFT JOIN	dbo.x_contract_udf xcu   on xcu.contract_sid = xc.contract_sid
			LEFT JOIN	dbo.c_udkey_8			cu8 on cu8.udkey_8_sid = xcu.udf_value
			where xcu.udf_sid = 84

			UPDATE #x_contract
			SET  PG = (CASE WHEN xcu.udf_value = 31 THEN ''Y'' ELSE ''N'' END)
			--select CASE WHEN xcu.udf_value IS NOT NULL THEN ''Yes'' ELSE ''Non'' END AS PG
			from #x_contract xc
			LEFT JOIN	dbo.x_contract_udf xcu   on xcu.contract_sid = xc.contract_sid
			where xcu.udf_sid IN (188,203) AND xcu.udf_value IS NOT NULL
			'
			EXEC (@SQL)

			UPDATE #x_contract
			SET Contribution_Type = 'None'
			FROM #x_contract
			where Contribution_Type is null

