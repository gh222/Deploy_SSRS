		CREATE TABLE #Duplicate_Products (contract_sid int, udkey_1_sid int, cnt int,Product_ID nvarchar(30), Product_Description nvarchar(60),Product_List nvarchar(60))
		INSERT INTO #Duplicate_Products(contract_sid , udkey_1_sid , cnt ,Product_ID , Product_Description ,Product_List )
		select ps.contract_sid, ps.udkey_1_sid, count(*) as cnt, a.Product_ID, a.Product_Description, a.Product_List
		from		#Product_Share		ps
		INNER JOIN #Attached_Products	a on ps.contract_sid = a.contract_sid and ps.udkey_1_sid = a.udkey_1_sid
		GROUP BY ps.contract_sid, ps.udkey_1_sid, a.Product_ID, a.Product_Description, a.Product_List
		HAVING count(*) &lt;&gt; 1

	
		DELETE FROm #Attached_Products
		from			#Attached_Products	cte 				
		INNER JOIN		#Duplicate_Products 	d on cte.contract_sid = d.contract_sid and cte.udkey_1_sid = d.udkey_1_sid
		
		INSERT INTO #Attached_Products (contract_sid, udkey_1_sid, Product_ID, Product_Description,seq_nbr ,Product_Share, sort_order_nbr,Default_Type,Duplicate_PS)
		select d.contract_sid, d.udkey_1_sid, d.Product_ID, d.Product_Description,sort_order_nbr as seq_nbr ,CAST(CAST(p.Product_Share as float) as decimal(18,2)) as Product_Share, sort_order_nbr, 0 as Default_Type,1 as Duplicate_PS
		from		#Duplicate_Products d
		LEFT JOIN	#Product_Share		p on p.contract_sid = d.contract_sid and p.udkey_1_sid = d.udkey_1_sid
	
				
		/*****************************************************************************************************/
