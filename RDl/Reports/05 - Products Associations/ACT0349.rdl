								INNER JOIN		#Product_Filter			pf on pf.udkey_1_sid = xcr.udkey_1_sid
								UNION 
								select    xcr.contract_sid,  cu1_2.udkey_1_sid  AS udkey_1_sid, xcr.udkey_1_group_sid,  cu1.udkey_1_id AS udkey_1_id
								from			dbo.x_contract_rights		xcr
								INNER JOIN		#x_contract				xc on xc.contract_sid = xcr.contract_sid
								LEFT JOIN		dbo.c_udkey_1_group_dtl		cu1_2 on cu1_2.udkey_1_group_sid = xcr.udkey_1_group_sid
								LEFT JOIN		dbo.c_udkey_1				cu1 on cu1.udkey_1_sid = cu1_2.udkey_1_sid
								INNER JOIN		#Product_Filter			pf on pf.udkey_1_sid = cu1_2.udkey_1_sid
								UNION 
								select  xcu.contract_sid,   xcu.udkey_1_sid AS udkey_1_sid, xcu.udkey_1_group_sid,  cu1.udkey_1_id AS udkey_1_id
								from dbo.x_contract_udf_lookup xcu
								INNER JOIN		#x_contract				xc on xc.contract_sid = xcu.contract_sid
								LEFT JOIN		dbo.c_udkey_1				cu1 on cu1.udkey_1_sid = xcu.udkey_1_sid
								INNER JOIN		#Product_Filter			pf on pf.udkey_1_sid = xcu.udkey_1_sid
								where xcu.udf_sid IN (13,99,202)
								UNION 
								select  xcu.contract_sid,   cu1_2.udkey_1_sid  AS udkey_1_sid, xcu.udkey_1_group_sid,  cu1.udkey_1_id  AS udkey_1_id
								from dbo.x_contract_udf_lookup xcu
								INNER JOIN		#x_contract				xc on xc.contract_sid = xcu.contract_sid
								LEFT JOIN		dbo.c_udkey_1_group_dtl		cu1_2 on cu1_2.udkey_1_group_sid = xcu.udkey_1_group_sid
								LEFT JOIN		dbo.c_udkey_1				cu1 on cu1.udkey_1_sid = cu1_2.udkey_1_sid
								INNER JOIN		#Product_Filter			pf on pf.udkey_1_sid = cu1_2.udkey_1_sid
								where xcu.udf_sid IN (13,99,202)
						'
						END
	

							IF (@Product_ID = 'All') 
