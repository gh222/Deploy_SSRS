								from dbo.x_contract_udf_lookup xcu
								INNER JOIN		#x_contract				xc on xc.contract_sid = xcu.contract_sid
								LEFT JOIN		dbo.c_udkey_1_group_dtl		cu1_2 on cu1_2.udkey_1_group_sid = xcu.udkey_1_group_sid
