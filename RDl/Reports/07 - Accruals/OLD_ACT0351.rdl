          </QueryParameter>
        </QueryParameters>
        <CommandText>IF( @Method_Selected_Input NOT IN ('3_Accrual_Validation_Summary','1_Set_Criteria_n_Schedule'))
		BEGIN
		Select 'None' as Royalty_Period
		END
ELSE
		BEGIN
			DECLARE @Date1 DATE, @Date2 DATE
			SET @Date1 = DATEADD(MM,-4,GETDATE())
			SET @Date2 = DATEADD(MM,3,GETDATE())

			;WITH CTE AS
			(
			SELECT LEFT(DATEADD(MM,number+1,@Date1),7) AS Royalty_Period
			FROM master..spt_values
			WHERE type = 'P' AND DATEADD(MM,number+1,@Date1) &lt; @Date2
			)select '2014-12' as Royalty_Period
			UNION ALL
			select Royalty_Period
			from CTE
		END</CommandText>
      </Query>
      <Fields>
        <Field Name="Royalty_Period">
          <DataField>Royalty_Period</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
