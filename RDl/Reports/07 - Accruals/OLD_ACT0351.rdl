    <DataSet Name="Config_Table">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Method_Selected_Input">
            <Value>=Parameters!Method_Selected_Input.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF( @Method_Selected_Input NOT IN ('3_Accrual_Validation_Summary','1_Set_Criteria_n_Schedule'))
		BEGIN
		Select 'None' as [Caspar_Layer_Name]
		END
ELSE
	BEGIN	
		;WITH CTE AS
		(
			SELECT [Caspar_Layer_Name]
			FROM [SSIS].[Configurations]
			where turn_on = 1 
			UNION 
			SELECT [Caspar_Layer_Name] 
			FROM [SSIS].[Configurations]
			where [Caspar_Layer_Type] = 'CRA'-- AND [Caspar_Layer_Name] = 'CRA WGB01DB7047 UAT'
  )
  select [Caspar_Layer_Name]
  from CTE
  ORDER BY 
	CASE WHEN [Caspar_Layer_Name] = 'CRA WGB01DB7047 UAT' THEN 1 ELSE 2 END
END</CommandText>
      </Query>
      <Fields>
        <Field Name="Caspar_Layer_Name">
          <DataField>Caspar_Layer_Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
