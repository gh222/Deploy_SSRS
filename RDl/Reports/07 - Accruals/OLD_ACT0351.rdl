UPDATE [SSIS].[Configurations] SET Run_Costs				= @Run_Costs				where [Caspar_Layer_Name] = @Caspar_Layer_Name
UPDATE [SSIS].[Configurations] SET Run_Selected_Contracts	= @Run_Selected_Contracts	where [Caspar_Layer_Name] = @Caspar_Layer_Name
