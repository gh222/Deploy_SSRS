        <CommandText>

----/*************** User Variables For Report ************************/
DECLARE @Method_Selected nvarchar(30)		SET @Method_Selected = @Method_Selected_Input
DECLARE @Caspar_Layer_Name nvarchar(30)		SET @Caspar_Layer_Name = @Caspar_Layer_Name_Input
DECLARE @Royalty_Period nvarchar(30)		SET @Royalty_Period = @Royalty_Period_Input
DECLARE @Run_Selected_Contracts	bit			SET @Run_Selected_Contracts = @Run_Selected_Contracts_Input

DECLARE @Run_Current_Accural bit			SET @Run_Current_Accural = @Run_Current_Accural_Input
DECLARE @Run_Previous_Accural bit			SET @Run_Previous_Accural = @Run_Previous_Accural_Input
DECLARE @Run_Sales_ITD	 bit				SET @Run_Sales_ITD = @Run_Sales_ITD_Input

DECLARE @Run_Costs	bit						SET @Run_Costs = @Run_Costs_Input
DECLARE @Run_Payaways bit					SET @Run_Payaways = @Run_Payaways_Input
DECLARE @Run_Contracts bit					SET @Run_Contracts = @Run_Contracts_Input
DECLARE @Run_Lists bit						SET @Run_Lists = @Run_Lists_Input
DECLARE @Update_Table_Variable nvarchar(30)	SET @Update_Table_Variable = @Update_Table_Variable_Input
DECLARE @ExcpetionRule_Month_On_Month_Min nvarchar(10) SET @ExcpetionRule_Month_On_Month_Min = @ExcpetionRule_Month_On_Month_Min_Input
DECLARE @ExcpetionRule_Month_On_Month_Max nvarchar(10) SET @ExcpetionRule_Month_On_Month_Max = @ExcpetionRule_Month_On_Month_Max_Input
DECLARE @ExcpetionRule_Top_Selling nvarchar(10) SET @ExcpetionRule_Top_Selling = @ExcpetionRule_Top_Selling_Input
DECLARE @Date_to_Run_Package datetime		SET @Date_to_Run_Package = @Date_to_Run_Package_Import


SET NOCOUNT ON;

/*
 
select '1_Set_Criteria_n_Schedule' AS Label
UNION ALL
select '2_Check_If_Packages_Are_Running' AS Label
UNION ALL
select '3_Accrual_Validation_Summary' AS Label
UNION ALL
select '4_Creator_Accrual_Detail' AS Label
UNION ALL
select '5_Creator_Accrual_Top_Up ' AS Label
*/

--/*************** User Variables Testing ************************/
--DECLARE @Caspar_Layer_Name nvarchar(30)		SET @Caspar_Layer_Name = 'CRA WGB01DB7047 UAT'
--DECLARE @Method_Selected nvarchar(60)		SET @Method_Selected = '1_Set_Criteria_n_Schedule'  -- Set_Criteria  Accrual_Validation_Summary  Nightly_Run_Dates Accrual_Top_Up  Check_If_Packages_Are_Running
--DECLARE @Royalty_Period nvarchar(30)		SET @Royalty_Period = '2014-12'
--DECLARE @Run_Selected_Contracts	bit			SET @Run_Selected_Contracts = 0

--/*************** Agent To Run These The NIght Before ************************/
--DECLARE @Run_Current_Accural bit			SET @Run_Current_Accural = 0
--DECLARE @Run_Previous_Accural bit			SET @Run_Previous_Accural = 0
--DECLARE @Run_Sales_ITD	 bit				SET @Run_Sales_ITD = 0

--/*************** Do Not Change As These always Run ************************/
--DECLARE @Run_Costs	bit						SET @Run_Costs = 1
--DECLARE @Run_Payaways bit					SET @Run_Payaways = 1 -- Broken
--DECLARE @Run_Contracts bit					SET @Run_Contracts = 1
--DECLARE @Run_Lists bit						SET @Run_Lists = 1
--DECLARE @Update_Table_Variable nvarchar(30)	SET @Update_Table_Variable = 'None'

--/*************** Set Criteria  ************************/
--DECLARE @ExcpetionRule_Month_On_Month_Min nvarchar(10) SET @ExcpetionRule_Month_On_Month_Min = '-500'
--DECLARE @ExcpetionRule_Month_On_Month_Max nvarchar(10) SET @ExcpetionRule_Month_On_Month_Max = '30000'
--DECLARE @ExcpetionRule_Top_Selling nvarchar(10) SET @ExcpetionRule_Top_Selling = '20'
--DECLARE @Date_to_Run_Package datetime		SET @Date_to_Run_Package = GETDATE()+1
/*************** Defaults ************************/

IF (@Caspar_Layer_Name = 'None') BEGIN		SET @Caspar_Layer_Name = 'CRA WGB01DB7047 UAT'  END 
IF (@Royalty_Period = 'None') BEGIN			SET @Royalty_Period = '2014-12'  END


/************ Update Config Table ******************************************/

IF (@Method_Selected IN ('1_Set_Criteria_n_Schedule','3_Accrual_Validation_Summary'))
BEGIN
UPDATE [SSIS].[Configurations] SET [Turn_On]				= 0
UPDATE [SSIS].[Configurations] SET [Turn_On]				= 1							where [Caspar_Layer_Name] = @Caspar_Layer_Name
