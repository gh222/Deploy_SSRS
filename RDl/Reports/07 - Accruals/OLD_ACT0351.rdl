IF (@Method_Selected = '1_Set_Criteria_n_Schedule')
BEGIN
	
		DECLARE @Schedule_Name_Import nvarchar(30) SET @Schedule_Name_Import = (select msdb.dbo.sysschedules.name from msdb.dbo.sysschedules where msdb.dbo.sysschedules.name = 'Accrual_Validation_Schedule')
		DECLARE @Schedule_Enable_Import nvarchar(1) SET @Schedule_Enable_Import = 1
		DECLARE @active_end_date_Import nvarchar(8) SET @active_end_date_Import = YEAR(@Date_to_Run_Package) * 10000 + 		MONTH(@Date_to_Run_Package) * 100 +		DAY(@Date_to_Run_Package+1)
		DECLARE @active_start_date_Import nvarchar(8) SET @active_start_date_Import = YEAR(@Date_to_Run_Package) * 10000 + 		MONTH(@Date_to_Run_Package) * 100 +		DAY(@Date_to_Run_Package)
		DECLARE @active_start_time_Import nvarchar(6) SET @active_start_time_Import = 030303
		DECLARE @freq_type_Import nvarchar(1) SET @freq_type_Import = 1 -- Once
		DECLARE @SQL nvarchar(1000)
		-- Update Schedule 
		SET @SQL = '
				EXEC msdb.dbo.sp_update_schedule
				@name = '''+@Schedule_Name_Import+''',
				@enabled = '+@Schedule_Enable_Import+',
				@active_end_date = '+@active_end_date_Import+',
				@active_start_date = '+@active_start_date_Import+',
				@freq_type = '+@freq_type_Import+',
				@active_start_time = '+@active_start_time_Import+'
		'
		EXEC (@SQL)

	
		TRUNCATE TABLE [CRA].[Validation_Summary] 

		INSERT INTO [CRA].[Validation_Summary](Caspar_Layer_Name,Royalty_Period, [Package_Name]      ,[Package_Start_Time]     ,[Package_Schedule]      ,[Package_Enabled]      ,[Package_End_Date],[Exception_Details])
		SELECT 
		(select [Caspar_Layer_Name] from [SSIS].[Configurations] where turn_on = 1)  as Caspar_Layer_Name
		,(select royalty_period from [SSIS].[Configurations] where turn_on = 1)  as Royalty_Period
		,msdb.dbo.sysjobs.name as Package_Name,
		 CAST(msdb.dbo.sysschedules.active_start_time / 10000 AS VARCHAR(10))   
		+ ':' + RIGHT('00' + CAST(msdb.dbo.sysschedules.active_start_time % 10000 / 100 AS VARCHAR(10)), 2) AS Package_Start_Time,  
		msdb.dbo.udf_schedule_description(msdb.dbo.sysschedules.freq_type, 
		msdb.dbo.sysschedules.freq_interval,  
		msdb.dbo.sysschedules.freq_subday_type, 
		msdb.dbo.sysschedules.freq_subday_interval, 
		msdb.dbo.sysschedules.freq_relative_interval,  
		msdb.dbo.sysschedules.freq_recurrence_factor, 
		msdb.dbo.sysschedules.active_start_date, 
		msdb.dbo.sysschedules.active_end_date,  
		msdb.dbo.sysschedules.active_start_time, 
		msdb.dbo.sysschedules.active_end_time) AS Package_Schedule, 
		
		msdb.dbo.sysschedules.enabled as Package_Enabled  
		,msdb.dbo.sysschedules.active_end_date AS Package_End_Date
		, 'Exception_Min: ' + (select [ExcpetionRule_Month_On_Month_Min] from [SSIS].[Configurations] where turn_on = 1) 
										+ ' ,Exception_Max: ' + (select [ExcpetionRule_Month_On_Month_Max] from [SSIS].[Configurations] where turn_on = 1) 
										+ ' ,Exception_Top_Selling: ' + (select [ExcpetionRule_Top_Selling] from [SSIS].[Configurations] where turn_on = 1) as [Exception_Details]
		
		FROM msdb.dbo.sysjobs INNER JOIN  
		msdb.dbo.sysjobschedules ON msdb.dbo.sysjobs.job_id = msdb.dbo.sysjobschedules.job_id INNER JOIN  
		msdb.dbo.sysschedules ON msdb.dbo.sysjobschedules.schedule_id = msdb.dbo.sysschedules.schedule_id 
		where msdb.dbo.sysjobs.name = 'Caspar_Warehouse_CRA_Accrual_Validation_Scheduled'
END


