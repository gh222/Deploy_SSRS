        <CommandText>

IF( @Method_Selected_Input &lt;&gt; '1_Set_Criteria_n_Schedule')
	BEGIN
	select 0 AS Minn
END
ELSE 
BEGIN
IF OBJECT_ID('tempdb..#Minn') IS NOT NULL DROP TABLE #Minn
CREATE TABLE #Minn (Minn int)
DECLARE @counter int SET @counter = -30000
WHILE (@counter &lt; -1)
BEGIN
INSERT INTO #Minn (Minn)
select CAST(@counter as nvarchar(10)) as Minn
SET @counter= @counter + 500
END
select Minn from #Minn order by Minn desc
END
</CommandText>
      </Query>
