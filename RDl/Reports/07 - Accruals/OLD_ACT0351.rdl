IF (@Method_Selected = '2_Check_If_Packages_Are_Running')
BEGIN
		TRUNCATE TABLE [CRA].[Validation_Summary] 

		INSERT INTO [CRA].[Validation_Summary](Caspar_Layer_Name,Royalty_Period,[Package_Name]      ,[Package_Start_Time]     ,[Package_Schedule]      ,[Package_Enabled]      ,[Package_End_Date],[Exception_Details])
		select 	(select [Caspar_Layer_Name] from [SSIS].[Configurations] where turn_on = 1)  as Caspar_Layer_Name
		,(select royalty_period from [SSIS].[Configurations] where turn_on = 1)  as Royalty_Period
		,'If Two lines here then package still running' as Package_Name, 1 as Package_Start_Time, 1 as [Package_Schedule], 1 as [Package_Enabled], 1 as [Package_End_Date]
		, 'Exception_Min: ' + (select [ExcpetionRule_Month_On_Month_Min] from [SSIS].[Configurations] where turn_on = 1) 
										+ ' ,Exception_Max: ' + (select [ExcpetionRule_Month_On_Month_Max] from [SSIS].[Configurations] where turn_on = 1) 
										+ ' ,Exception_Top_Selling: ' + (select [ExcpetionRule_Top_Selling] from [SSIS].[Configurations] where turn_on = 1) as [Exception_Details]
		UNION ALL
		select 	(select [Caspar_Layer_Name] from [SSIS].[Configurations] where turn_on = 1)  as Caspar_Layer_Name
		,(select royalty_period from [SSIS].[Configurations] where turn_on = 1)  as Royalty_Period
		,name as Package_Name, run_requested_date as Package_Start_Time, 1 as [Package_Schedule], 1 as [Package_Enabled], 1 as [Package_End_Date]
		, 'Exception_Min: ' + (select [ExcpetionRule_Month_On_Month_Min] from [SSIS].[Configurations] where turn_on = 1) 
										+ ' ,Exception_Max: ' + (select [ExcpetionRule_Month_On_Month_Max] from [SSIS].[Configurations] where turn_on = 1) 
										+ ' ,Exceptionn_Top_Selling: ' + (select [ExcpetionRule_Top_Selling] from [SSIS].[Configurations] where turn_on = 1) as [Exception_Details]
	
		from msdb.dbo.sysjobs_view job  
		inner join msdb.dbo.sysjobactivity activity on job.job_id = activity.job_id 
		where  
				activity.run_Requested_date is not null  
				and activity.stop_execution_date is null  
END


/*********************************************************************************************************************************/
/*********************************************************************************************************************************/
/************ Run Accrual_Validation_Summary *************************************************************************************/
/*********************************************************************************************************************************/
/*********************************************************************************************************************************/

IF (@Method_Selected = '3_Accrual_Validation_Summary')
BEGIN
		/**************************************************************************************************************/
		/************ Check if a SQL Server Agent job is running ******************************************************/
		/**************************************************************************************************************/

			DECLARE @JOB_NAME SYSNAME = N'Caspar_Warehouse_CRA_Accrual_Validation_Start'; 
 			IF NOT EXISTS(     
					select 1
					from msdb.dbo.sysjobs_view job  
					inner join msdb.dbo.sysjobactivity activity on job.job_id = activity.job_id 
					where  
						activity.run_Requested_date is not null  
					and activity.stop_execution_date is null  
					and job.name = @JOB_NAME 
					) 
			BEGIN      
				--PRINT 'Starting job ''' + @JOB_NAME + ''''; 
				RAISERROR ('Starting job', 0, 1) WITH NOWAIT
				EXEC msdb.dbo.sp_start_job @JOB_NAME; 
			END 
			ELSE 
			BEGIN 
				PRINT 'Job ''' + @JOB_NAME + ''' is already started '; 
			END 

		/**************************************************************************************************************/
		/************ Check if a SQL Server Agent job FINISHED*********************************************************/
		/**************************************************************************************************************/

		DECLARE @While_Loop_Stopper int SET @While_Loop_Stopper = 1
		RAISERROR ('Just Before loop Run', 0, 1) WITH NOWAIT	
		WHILE (@While_Loop_Stopper = 1)
		BEGIN
 			    SET @While_Loop_Stopper = (
					select Count(*)
					from msdb.dbo.sysjobs_view job  
					inner join msdb.dbo.sysjobactivity activity on job.job_id = activity.job_id 
					where  
						activity.run_Requested_date is not null  
					and activity.stop_execution_date is null  
					and job.name =  @JOB_NAME )

					-- Report will fail after looping for 10 minutes, this occurs when large package is run. User best to return in hours time
					IF( (select CAST(Run_Current_Accural as int) +CAST(Run_Previous_Accural as int) from [SSIS].[Configurations] where Turn_On = 1) &gt;0) 
					BEGIN 
						TRUNCATE TABLE [CRA].[Validation_Summary] 
						INSERT INTO [CRA].[Validation_Summary]([Package_Name]      ,[Package_Start_Time]     ,[Package_Schedule]      ,[Package_Enabled]      ,[Package_End_Date])
						select 'Please run 2_Check_If_Packages_Are_Running in a hours time' as Package_Name, 1 as Package_Start_Time, 1 as [Package_Schedule], 1 as [Package_Enabled], 1 as [Package_End_Date]
						BREAK; 
					END

				WAITFOR DELAY '00:00:30';
				RAISERROR ('in loop', 0, 1) WITH NOWAIT	
		END
				PRINT 'Loop Finished ''' + @JOB_NAME + ''''; 
				UPDATE [SSIS].[Configurations]
				SET  [Run_Payaways] = 0
					  ,[Run_Costs] = 0
					  ,[Run_Contracts] = 0
					  ,[Run_Lists]  = 0
					  ,[Run_Current_Accural] = 0
					  ,[Run_Sales_ITD] = 0
					  ,[Run_Previous_Accural] = 0
					  ,[Run_Selected_Contracts] = 0
					  ,[Update_Table_Variable] = 'None'
				  where turn_on = 1
				
END	

IF (@Method_Selected = '4_Creator_Accrual_Detail')
BEGIN
		select top 1 * FROM [CRA].[Validation_Summary]
END

IF (@Method_Selected = '5_Creator_Accrual_Top_Up')
