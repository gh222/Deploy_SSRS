      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Method_Selected_Input">
            <Value>=Parameters!Method_Selected_Input.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>IF( @Method_Selected_Input &lt;&gt; '1_Set_Criteria_n_Schedule')
	BEGIN
	select 0 AS Maxx
END
ELSE 
BEGIN
IF OBJECT_ID('tempdb..#Maxx') IS NOT NULL DROP TABLE #Maxx
CREATE TABLE #Maxx (Maxx int)

DECLARE @counter int SET @counter = 2000
WHILE (@counter &lt; 50000)
BEGIN
INSERT INTO #Maxx (Maxx)
select CAST(@counter as nvarchar(10)) as Maxx
SET @counter= @counter + 2000
END
select Maxx from #Maxx
END


</CommandText>
      </Query>
