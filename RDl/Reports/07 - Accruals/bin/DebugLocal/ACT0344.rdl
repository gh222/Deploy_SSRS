  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Contract_Id_Input">
            <Value>=Parameters!Contract_Id_Input.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Current_Period_Accrual_Input">
            <Value>=Parameters!Current_Period_Accrual_Input.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Refresh_Brand_Input">
            <Value>=Parameters!Refresh_Brand_Input.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@top_Input">
            <Value>=Parameters!top_Input.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>

SET TRANSACTION ISOLATION LEVEL READ UNCOMMITTED
SET NOCOUNT ON

DECLARE @Contract_Id varchar(3000)					SET @Contract_Id					= 'FPI016238 DENHAM' -- All --'FPI016238 DENHAM,TOPGEAR003 NEW,FPI014922 JAM NEW,FPI015700,SUDDEUT004,UNICORN002,FPI014228,FPI015692 INGENIOUS,DENHAM123A,FPI015070 VIDEO ROW'
DECLARE @Current_Period_Accrual varchar(7)			SET @Current_Period_Accrual			= '2015-07'
DECLARE @Refresh_Brand nvarchar(3)					SET @Refresh_Brand					= 'No'
DECLARE @top nvarchar(10)							SET @top = 10

SET @Contract_Id				= @Contract_Id_Input
SET @Current_Period_Accrual		= @Current_Period_Accrual_Input
SET @Refresh_Brand				= @Refresh_Brand_Input
SET @top						= @top_Input


/**********************************************************************************************************/
/************************* Fix Paramters Brands*************************************************************/
/**********************************************************************************************************/

DECLARE @sql varchar(2000)
DECLARE @Previous_Period varchar(7)					SET @Previous_Period				= (CAST((CASE WHEN RIGHT(@Current_Period_Accrual,2)= '01' THEN LEFT(@Current_Period_Accrual,4)-1 ELSE LEFT(@Current_Period_Accrual,4) END) as nvarchar(4)) +'-'+ RIGHT(CASE WHEN RIGHT(@Current_Period_Accrual,2)= '01' THEN '12' ELSE '0'+CAST(RIGHT(@Current_Period_Accrual,2)-1 as nvarchar(2)) END,2))
DECLARE @Previous_Previous_Period varchar(7)		SET @Previous_Previous_Period		= (CAST((CASE WHEN RIGHT(@Previous_Period,2)= '01' THEN LEFT(@Previous_Period,4)-1 ELSE LEFT(@Previous_Period,4) END) as nvarchar(4)) +'-'+ RIGHT(CASE WHEN RIGHT(@Previous_Period,2)= '01' THEN '12' ELSE '0'+CAST(RIGHT(@Previous_Period,2)-1 as nvarchar(2)) END,2))
DECLARE @Current_Period_Accrual_sid int				SET @Current_Period_Accrual_sid		= (select period_sid from x_period where period_id = @Current_Period_Accrual)
DECLARE @Previous_Period_sid int					SET @Previous_Period_sid			= (select period_sid from x_period where period_id = @Previous_Period)
DECLARE @Previous_Previous_Period_sid int			SET @Previous_Previous_Period_sid	= (select period_sid from x_period where period_id = @Previous_Previous_Period)
DECLARE @Job_Name_Input nvarchar(100)

/**********************************************************************************************************/
/************************* Get Brands**********************************************************************/
/**********************************************************************************************************/

	IF (@Refresh_Brand = 'Yes')
	BEGIN
		exec   WGB01DB7049.[Caspar_Warehouse_Supporting].[Report_Support].[usp_Package_Run] @Job_Name_Input ='Caspar_Warehouse_ACT0328_Brand'
	END
  
/**********************************************************************************************************/
/************************* Create Results Tables **********************************************************/
/**********************************************************************************************************/

			IF OBJECT_ID('tempdb..#Results') IS NOT NULL DROP TABLE #Results
			CREATE TABLE #Results
			(
			 contract_sid								int
			,Current_Period_Accrual_sid					int
			,deal_sid_accrual							int
			,x_11										nvarchar(1) DEFAULT '|'
			,Current_Period_Accrual						nvarchar(7)
			,Contract_Id								nvarchar(30) 
			,Contract_Description						nvarchar(30)
			,PG_Exists									bit
			,CBS_Exists									bit DEFAULT 0
			,Admin_Class								nvarchar(30)
			,Category									nvarchar(30)
			,Brand										nvarchar(30)
			,FPI										nvarchar(30)
			,Currency									nvarchar(3) DEFAULT 'GBP'
			,x_1										nvarchar(1) DEFAULT '|'
			,Creditor_ITD								decimal(18,2) DEFAULT 0
			,Earn_Out_ITD								decimal(18,2) DEFAULT 0
			,Broadcaster_ITD							decimal(18,2) DEFAULT 0
			,Accrual_Export_Total_SAPPPOGL				AS (Creditor_ITD+ Broadcaster_ITD+ Earn_Out_ITD  )
			,x_1141										nvarchar(1) DEFAULT '|'
			,Creditor_Prior_ITD							decimal(18,2) DEFAULT 0
			,Earn_Out_Prior_ITD							decimal(18,2) DEFAULT 0
			,Broadcaster_Prior_ITD						decimal(18,2) DEFAULT 0
			,Accrual_Prior_Total_Export_SAPPPOGL		AS (Creditor_Prior_ITD+ Broadcaster_Prior_ITD+ Earn_Out_Prior_ITD  )
			,x_111										nvarchar(1) DEFAULT '|'
			,Creditor_Variance							AS (Creditor_ITD - Creditor_Prior_ITD)
			,Earn_Out_Variance							AS (Earn_Out_ITD - Earn_Out_Prior_ITD)
			,Broadcaster_Variance						AS (Broadcaster_ITD - Broadcaster_Prior_ITD)
			,Total_Variance								AS (Creditor_ITD+ Broadcaster_ITD+ Earn_Out_ITD   ) - (Creditor_Prior_ITD+ Broadcaster_Prior_ITD+ Earn_Out_Prior_ITD   )
			,x_1s11										nvarchar(1) DEFAULT '|'
			,Investment_ITD								decimal(18,2) DEFAULT 0
			,Investment_Prior_ITD						decimal(18,2) DEFAULT 0
			,ITD_PG										decimal(18,2) DEFAULT 0
			,ITD_Previous_PG							decimal(18,2) DEFAULT 0
			,x_4										nvarchar(1) DEFAULT '|'
			,Pymt_Creditor_CUR_Current_Month			decimal(18,2) DEFAULT 0
			,Pymt_LockOff_CUR_Current_Month_SAPPOEA		decimal(18,2) DEFAULT 0
			,Pymt_Broadcaster_CUR_Current_Month			decimal(18,2) DEFAULT 0
			,Pymt_Period_End							nvarchar(30) DEFAULT 'None'
			,Pymt_Approved_Date							date DEFAULT '01/01/1990'
			,Pymt_Total_CUR_Current_Month				AS (Pymt_Creditor_CUR_Current_Month+ Pymt_LockOff_CUR_Current_Month_SAPPOEA+ Pymt_Broadcaster_CUR_Current_Month )
			,x_43										nvarchar(1) DEFAULT '|'
			,Accruals_ITD_plus_Pymt_CUR_Current_Month	AS (Creditor_ITD + Earn_Out_ITD+ Broadcaster_ITD  +Pymt_Creditor_CUR_Current_Month+ Pymt_Broadcaster_CUR_Current_Month +Pymt_LockOff_CUR_Current_Month_SAPPOEA)
			,x_2										nvarchar(1) DEFAULT '|'
			,YTD_Creditor								decimal(18,2) DEFAULT 0
			,YTD_CBS_Payments							decimal(18,2) DEFAULT 0
			,YTD_LockOff_Payments						decimal(18,2) DEFAULT 0
			,YTD_Total									AS YTD_Creditor + YTD_CBS_Payments + YTD_LockOff_Payments
			,YTD_Period_End								nvarchar(100) DEFAULT ''
			,x_463										nvarchar(1) DEFAULT '|'
			,Prior_Month_YTD_Total						AS (YTD_Creditor + YTD_CBS_Payments + YTD_LockOff_Payments) - (Pymt_Creditor_CUR_Current_Month+ Pymt_Broadcaster_CUR_Current_Month +Pymt_LockOff_CUR_Current_Month_SAPPOEA   )
			,YTD_Total_Variance							AS (Pymt_Creditor_CUR_Current_Month+ Pymt_Broadcaster_CUR_Current_Month +Pymt_LockOff_CUR_Current_Month_SAPPOEA   )
			)
