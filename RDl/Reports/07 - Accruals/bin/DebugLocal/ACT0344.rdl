		IF (@Contract_Id &lt;&gt; 'All')	BEGIN 
			SET @Contract_Id = ''''+REPLACE(@Contract_Id,',',''',''')+''''
			SET @sql =
			'
			INSERT INTO #Results (Current_Period_Accrual, Current_Period_Accrual_sid, contract_sid, Contract_Id, Contract_Description,Admin_Class) 	
			select '''+CAST(@Current_Period_Accrual as nvarchar(30))+''' as Current_Period_Accrual, '''+CAST(@Current_Period_Accrual_sid as nvarchar(30))+''' as Current_Period_Accrual_sid, contract_sid, LTRIM(RTRIM(Contract_Id)) as Contract_Id, REPLACE(REPLACE(REPLACE(REPLACE(LEFT(xc.descr,30),'','',''''),CHAR(10),''''),CHAR(13),''''),CHAR(9),'''')   as Contract_Description 	, xac.admin_class_id   as Admin_Class	
			from x_contract xc 	
			INNER JOIN x_admin_class xac on xac.admin_class_sid = xc.admin_class_sid	
			where contract_id IN ('+@Contract_Id+') and  contract_sid = original_contract_sid 
			'
			exec (@sql)
			END

		IF (@Contract_Id = 'All')	
		BEGIN 
				INSERT INTO #Results (Current_Period_Accrual, Current_Period_Accrual_sid, contract_sid, Contract_Id, Contract_Description,Admin_Class) 	
				select @Current_Period_Accrual, @Current_Period_Accrual_sid, contract_sid, LTRIM(RTRIM(Contract_Id)) as Contract_Id, REPLACE(REPLACE(REPLACE(REPLACE(LEFT(xc.descr,30),',',''),CHAR(10),''),CHAR(13),''),CHAR(9),'') as Contract_Description 	, xac.admin_class_id  as Admin_Class	
				from			x_contract			xc 	
				INNER JOIN		x_admin_class		xac on xac.admin_class_sid = xc.admin_class_sid	
				where contract_sid = original_contract_sid 
		
				INSERT INTO #Results (Current_Period_Accrual, Current_Period_Accrual_sid, contract_sid, Contract_Id, Contract_Description,Admin_Class) 	
				select @Current_Period_Accrual, @Current_Period_Accrual_sid, contract_sid, LTRIM(RTRIM(Contract_Id)) as Contract_Id, REPLACE(REPLACE(REPLACE(REPLACE(LEFT(xc.descr,30),',',''),CHAR(10),''),CHAR(13),''),CHAR(9),'') as Contract_Description, xac.admin_class_id   as Admin_Class
				from x_contract xc 
				INNER JOIN		x_admin_class		xac on xac.admin_class_sid = xc.admin_class_sid	
				where contract_id NOT IN (	select contract_id from #Results)
				--and Contract_Id = 'FPI019582'
		END	

		UPDATE #Results  SET deal_sid_accrual = xd.deal_sid
		from			#Results	c
		INNER JOIN		x_deal		xd on xd.contract_sid = c.contract_sid
		where xd.deal_id LIKE '%Accru%'
		CREATE CLUSTERED INDEX XI_Results ON #Results(deal_sid_accrual)

