		--RAISERROR ('Accrual Detail Start', 0, 1) WITH NOWAIT
		IF OBJECT_ID('tempdb..#Detail_Accrual') IS NOT NULL DROP TABLE #Detail_Accrual
		CREATE TABLE #Detail_Accrual
		(
		 deal_sid_accrual			int
		,Royalty_period_sid int
		,Sales_Period_sid	int
		,udkey_2_sid		int
		,udkey_3_sid		int
		,amount				decimal(18,2)
		)

		INSERT INTO #Detail_Accrual ( deal_sid_accrual,royalty_period_sid ,Sales_Period_sid		,udkey_2_sid,	udkey_3_sid, amount)
		select  xdcr.deal_sid as deal_sid_accrual,	xdcr.period_sid as royalty_period_sid ,xdcr.actual_period_sid as Sales_Period_sid	,xdcr.udkey_2_sid,	xdcr.udkey_3_sid,  sum(xdcr.amount) as amount
		from				x_deal_calc_result		xdcr
		INNER JOIN			#Period					p on xdcr.period_sid = p.royalty_period_sid	
		INNER JOIN			#Results				c on xdcr.deal_sid = c.deal_sid_accrual
		GROUP BY xdcr.deal_sid,	xdcr.period_sid  ,xdcr.actual_period_sid ,xdcr.udkey_2_sid,	xdcr.udkey_3_sid

		CREATE CLUSTERED INDEX XI_Detail ON #Detail_Accrual(deal_sid_accrual, udkey_2_sid)
		CREATE NONCLUSTERED INDEX XI_Accrual_Detail ON [#Detail_Accrual] ([udkey_2_sid],[udkey_3_sid]) INCLUDE ([deal_sid_accrual],[Royalty_period_sid],[amount])
	
			
		/**********************************************************************************************************/
		/************************* Update Accrual Results ITD *****************************************************/
		/**********************************************************************************************************/

				;WITH CTE_PivotData AS
						(
						SELECT
						d.deal_sid_accrual,-- cu2.Account_Type as Account_Type ,	-- grouping column
						CASE 
							WHEN d.Royalty_period_sid  = @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Creditor'	AND d.udkey_3_sid = 2	THEN 'Creditor_ITD' 
							WHEN d.Royalty_period_sid  &lt;&gt; @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Creditor'	AND d.udkey_3_sid = 2	THEN 'Creditor_Prior_ITD' 
							WHEN d.Royalty_period_sid  = @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Broadcaster'		AND d.udkey_3_sid = 2	THEN 'ITD_Broadcaster' 
							WHEN d.Royalty_period_sid  &lt;&gt; @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Broadcaster'		AND d.udkey_3_sid = 2	THEN 'ITD_Previous_Broadcaster' 
							WHEN d.Royalty_period_sid  = @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Investment'		AND d.udkey_3_sid = 2	THEN 'ITD_Investment' 
							WHEN d.Royalty_period_sid  &lt;&gt; @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Investment'		AND d.udkey_3_sid = 2	THEN 'ITD_Previous_Investment' 
							WHEN d.Royalty_period_sid  = @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Earn_Out'		AND d.udkey_3_sid = 2	THEN 'ITD_Earn_Out' 
							WHEN d.Royalty_period_sid  &lt;&gt; @Current_Period_Accrual_sid		AND cu2.Account_Type = 'Earn_Out'		AND d.udkey_3_sid = 2	THEN 'ITD_Previous_Earn_Out' 
							--WHEN d.Royalty_period_sid  = @Current_Period_Accrual_sid		AND cu2.Account_Type = 'CBS'			THEN 'ITD_CBS' 
							--WHEN d.Royalty_period_sid  &lt;&gt; @Current_Period_Accrual_sid		AND cu2.Account_Type = 'CBS'			THEN 'ITD_Previous_CBS' 			
							WHEN d.Royalty_period_sid  = @Current_Period_Accrual_sid		AND cu2.Account_Type = 'PG'				THEN 'ITD_PG' 
							WHEN d.Royalty_period_sid  &lt;&gt; @Current_Period_Accrual_sid		AND cu2.Account_Type = 'PG'				THEN 'ITD_Previous_PG' 			
							
							ELSE 'No Account' END AS Type_,				-- spreading column
						SUM(d.amount) as amount								-- aggregation column
						from			#Detail_Accrual d  
						INNER JOIN		#Accounts_ITD_Accrual cu2 on cu2.udkey_2_sid = d.udkey_2_sid AND cu2.udkey_3_sid = d.udkey_3_sid 
						--where d.deal_sid_accrual = 162711
						GROUP BY d.deal_sid_accrual, d.Royalty_period_sid, d.udkey_3_sid , cu2.Account_Type--, d.udkey_2_sid
						)
				, CTE_Detail_To_Update as
						(
						SELECT deal_sid_accrual --, Account_Type
							, ISNULL([Creditor_ITD],0)								AS [Creditor_ITD]
							, ISNULL([Creditor_Prior_ITD],0)						AS [Creditor_Prior_ITD] 
							, ISNULL([ITD_Broadcaster],0)								AS [ITD_Broadcaster]
							, ISNULL([ITD_Previous_Broadcaster],0)						AS [ITD_Previous_Broadcaster]
							, ISNULL([ITD_Investment],0)							AS [ITD_Investment]
							, ISNULL([ITD_Previous_Investment],0)					AS [ITD_Previous_Investment]
							, ISNULL([ITD_Earn_Out],0)								AS [ITD_Earn_Out]
							, ISNULL([ITD_Previous_Earn_Out],0)						AS [ITD_Previous_Earn_Out]
							--, ISNULL([ITD_CBS],0)									AS [ITD_CBS]
							--, ISNULL([ITD_Previous_CBS],0)							AS [ITD_Previous_CBS]
							, ISNULL([ITD_PG],0)									AS [ITD_PG]
							, ISNULL([ITD_Previous_PG],0)							AS [ITD_Previous_PG]

						FROM CTE_PivotData piv
						    PIVOT(SUM(piv.amount) FOR piv.Type_ IN 
									( [Creditor_ITD]
									, [Creditor_Prior_ITD]   
									, [ITD_Broadcaster]
									, [ITD_Previous_Broadcaster]  
									, [ITD_Investment]
									, [ITD_Previous_Investment]
									, [ITD_Earn_Out]
									, [ITD_Previous_Earn_Out]
									--, [ITD_CBS]
									--, [ITD_Previous_CBS]
									, [ITD_PG]
									, [ITD_Previous_PG]
							) ) AS P
								)
										UPDATE #Results SET 
										  Creditor_ITD				= cte.Creditor_ITD
										, Creditor_Prior_ITD		= cte.Creditor_Prior_ITD
										, Broadcaster_ITD				= cte.ITD_Broadcaster
										, Broadcaster_Prior_ITD		= cte.ITD_Previous_Broadcaster
										, Investment_ITD		= cte.ITD_Investment
										, Investment_Prior_ITD	= cte.[ITD_Previous_Investment]
										, Earn_Out_ITD				= cte.ITD_Earn_Out
										, Earn_Out_Prior_ITD		= cte.ITD_Previous_Earn_Out
										--, CBS_ITD					= cte.ITD_CBS
										--, CBS_Prior_ITD				= cte.ITD_Previous_CBS
										, [ITD_PG]					= cte.[ITD_PG]
										, [ITD_Previous_PG]			= cte.[ITD_Previous_PG]
										from			CTE_Detail_To_Update	cte
										INNER JOIN		#Results				r on r.deal_sid_accrual = cte.deal_sid_accrual
	

