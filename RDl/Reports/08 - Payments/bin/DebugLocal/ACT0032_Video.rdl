--7219
update #results
set contact_id =	cc.contact_id 
					from	#results r
					inner join x_contract xc on r.contract_id = xc.contract_id
					inner join x_contract_participant xcp on xcp.contract_sid = xc.contract_sid
					inner join c_contact cc on cc.contact_sid = xcp.contact_sid
					where r.contact_id is null
					
					
update #results
set formatted_name = cc.formatted_name
					from	#results r
					inner join x_contract xc on r.contract_id = xc.contract_id
					inner join x_contract_participant xcp on xcp.contract_sid = xc.contract_sid
					inner join c_contact cc on cc.contact_sid = xcp.contact_sid
					where r.formatted_name is null
					
				
				
-- now sum together the two accounts 
if @rtn_code = 0
begin
	select 	approval_group,
		r.contract_id,
		contract,
		contact_id,
		formatted_name, 
--		product_id,
--		product,
		udkey_10_sid,
		currency,
		sum(amount) as amount
	from #results r, x_contract c
	where r.contract_id = c.contract_id
	and c.contracT_sid = c.original_contract_sid
	and c.status_code not in ('EXPIRED')
	group by approval_group,
		udkey_10_sid,		-- currency
		currency,
		r.contract_id,		-- contract
		contract,
		contact_id,		-- contact
		formatted_name
--		product_id,		-- product
--		product
	having sum(amount) != 0
end

drop table #results
drop table #approval
drop table #accounts
drop table #periods</CommandText>
