    <DataSet Name="PeriodEnd">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>SELECT   p1.period_id   ,
CONVERT(datetime,CONVERT(VARCHAR(24),period_ending_date, 102)+' 23:59:59') as 'PeriodEndDate'
,period_sid FROM  x_period p1 ORDER   BY p1.period_ending_date</CommandText>
      </Query>
