        <CommandText>declare @startdate datetime
declare @enddate datetime

set @startdate = @inputstartdate
set @enddate = @inputenddate

--add a minute to the start period so that we don't return the previous period
set @startdate = dateadd(n,1,@startdate)

if @enddate is null set @enddate =  '3000-01-01 00:00:00.000'
if @enddate= '' set @enddate =   '3000-01-01 00:00:00.000'

if @startdate is null set @startdate =  '1900-01-01 00:00:00.000'
if @startdate= '' set @startdate =   '1900-01-01 00:00:00.000'



create table #approval(
	group_sid sid,
	descr nvarchar(100)
)
insert into #approval select approval_group_sid, descr  from c_approval_group where descr in ('Statements','Subrights')

create table #accounts(
	account_sid sid
)

if @media = 'IPA' or @media='PPO'
begin
	insert into #accounts values(530)
end
else
begin
	insert into #accounts values(89)
	insert into #accounts values(390)
end

select period_sid 
into #periods
from x_period
where period_ending_date between @startdate and @enddate

/*
86	FINAL NEG EARNINGS BALANCE	Final Negative Earnings Balance	
89	PAYMENT AMT TO BE CF	Payment Amount To Be Carried Forward
105	FINAL ADVANCE BALANCE	Final Advance Balance
116	PAYMENT DUE - FINAL	Payment Due - Final
432	STMT SUM - CARRIED FWD	Statement Summary - Carried Forward	
476	FIXER FEE TO BE CF	Fixer Fee To Be Carried Forward
*/

declare @rtn_code int
set @rtn_code = 0


create table #results(
	approval_group descr,
	contract_id id,
	contract descr,
	contact_id id null,
	formatted_name varchar(255), 
	product_id id ,
	product descr,
	udkey_2_sid sid,
	account_id id,
	udkey_10_sid sid,
	currency descr,
	amount hc_amt
)

--select * from x_period order by period_id
if @approved = 1
begin
	insert into #results
	select	a.descr,
			xc.contract_id,
			xc.descr,
			cc.contact_id,
			cc.formatted_name,
			cu1.udkey_1_id as product_id,
			cu1.descr as product,
			xdcr.udkey_2_sid,
			cu2.udkey_2_id as account,
			xdcr.udkey_10_sid,
			cu10.descr as currency,
			sum(xdcr.amount) as amount
	from	x_contract xc
	inner join x_deal xd						on xd.contract_sid = xc.contract_sid
	inner join x_deal_calc_result xdcr			on xdcr.deal_sid = xd.deal_sid 
	inner join #periods xp						on xp.period_sid = xdcr.period_sid
	inner join #accounts cu2list				on cu2list.account_sid = xdcr.udkey_2_sid
	inner join #approval a						ON a.group_sid = xd.approval_group_sid
	inner join x_template_job_queue_hist tjq	ON xdcr.deal_sid = tjq.deal_sid	
	left join x_contract_participant xcp		on xcp.contract_sid = xc.contract_sid and xcp.contact_sid = xdcr.user_contact_sid
	left join c_contact cc						on cc.contact_sid = xcp.contact_sid
	left join c_udkey_1 cu1						ON cu1.udkey_1_sid = xdcr.udkey_1_sid
	left join c_udkey_2 cu2						ON cu2.udkey_2_sid = xdcr.udkey_2_sid
	left join c_udkey_10 cu10					ON cu10.udkey_10_sid = xdcr.udkey_10_sid
	where	xdcr.udkey_3_sid = 2
	and		xdcr.period_sid = tjq.processing_cutoff_period_sid
	and		tjq.calc_order_nbr = 1
	group by a.descr,
			xc.contract_id,
			xc.descr,
			cc.contact_id,
			cc.formatted_name,
			cu1.udkey_1_id,
			cu1.descr,
			xdcr.udkey_2_sid,
			cu2.udkey_2_id,
			xdcr.udkey_10_sid,
			cu10.descr
end
else
begin
	insert into #results
	select	a.descr,
			xc.contract_id,
			xc.descr,
			cc.contact_id,
			cc.formatted_name,
			cu1.udkey_1_id as product_id,
			cu1.descr as product,
			xdcr.udkey_2_sid,
			cu2.udkey_2_id as account,
			xdcr.udkey_10_sid,
			cu10.descr as currency,
			sum(xdcr.amount) as amount
	from	x_contract xc
	inner join x_deal xd						on xd.contract_sid = xc.contract_sid
	inner join x_deal_calc_result xdcr			on xdcr.deal_sid = xd.deal_sid
	inner join #periods xp						on xp.period_sid = xdcr.period_sid
	inner join #accounts cu2list				on cu2list.account_sid = xdcr.udkey_2_sid
	inner join #approval a						ON a.group_sid = xd.approval_group_sid
	inner join x_template_job_queue tjq			ON xdcr.deal_sid = tjq.deal_sid	
	left join x_contract_participant xcp		on xcp.contract_sid = xc.contract_sid and xcp.contact_sid = xdcr.user_contact_sid
	left join c_contact cc						on cc.contact_sid = xcp.contact_sid
	left join c_udkey_1 cu1						ON cu1.udkey_1_sid = xdcr.udkey_1_sid
	left join c_udkey_2 cu2						ON cu2.udkey_2_sid = xdcr.udkey_2_sid
	left join c_udkey_10 cu10					ON cu10.udkey_10_sid = xdcr.udkey_10_sid
	where	xdcr.udkey_3_sid = 2
	and		xdcr.period_sid = tjq.processing_cutoff_period_sid
	and		tjq.calc_order_nbr = 1
	group by a.descr,
			xc.contract_id,
			xc.descr,
			cc.contact_id,
			cc.formatted_name,
			cu1.udkey_1_id,
			cu1.descr,
			xdcr.udkey_2_sid,
			cu2.udkey_2_id,
			xdcr.udkey_10_sid,
			cu10.descr
end
if @@error &lt;&gt; 0 set @rtn_code = 1
