    <DataSet Name="PeriodStart">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>SELECT   p1.period_id   ,
ISNULL(p2.period_ending_date,'17530101') as 'PeriodStartDate' 
FROM  x_period p1 
LEFT JOIN x_period p2 ON p2.period_ending_date = (SELECT max(period_ending_date)	 
FROM x_period 	 WHERE period_ending_date &lt; p1.period_ending_date) 
ORDER   BY p1.period_ending_date</CommandText>
      </Query>
