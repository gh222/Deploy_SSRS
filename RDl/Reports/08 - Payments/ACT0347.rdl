    <DataSet Name="Keywords">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>;with CTE AS 
(
select descr As Keyword
from c_keyword
where descr &lt;&gt; ''
UNION ALL
select 'All' As Keyword
)
select *
from CTE
ORDER BY CASE
			 WHEN Keyword LIKE 'All' THEN '0'
             ELSE Keyword END ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="Keyword">
          <DataField>Keyword</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Preparer">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>
;with CTE AS 
(
select formatted_name as Preparer
from x_user
UNION ALL
select 'All' as Preparer
)
select *
from CTE
ORDER BY CASE
			 WHEN Preparer LIKE 'All' THEN '0'
			 WHEN Preparer LIKE 'PAYMENT FILE 1' THEN '1'
              WHEN Preparer LIKE 'PAYMENT FILE 2' THEN '2'
			  WHEN Preparer LIKE 'PAYMENT FILE 3' THEN '3'
			  WHEN Preparer LIKE 'PAYMENT FILE 4' THEN '4'
			  WHEN Preparer LIKE 'PAYMENT FILE 5' THEN '5'
			  WHEN Preparer LIKE 'PAYMENT FILE 6' THEN '6'
			  WHEN Preparer LIKE 'ALLIANT%' THEN '7'
			  WHEN Preparer LIKE '%PARTICIPANT%' THEN '8'
			  WHEN Preparer LIKE 'PRODUCERS GROSS' THEN '9'
			  WHEN Preparer LIKE 'TOP 50' THEN '9'
              ELSE Preparer END ASC</CommandText>
      </Query>
      <Fields>
        <Field Name="Preparer">
          <DataField>Preparer</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
