		DECLARE @Server_n_Database nvarchar(100) SET @Server_n_Database =''
		IF (@Report_Folder LIKE '%CRA V6.2%') BEGIN SET @Server_n_Database = 'WGB01DB7047.bbc_cra_prod'  END 
		IF (@Report_Folder LIKE '%Live%') BEGIN SET @Server_n_Database = 'WGB01DB1041.bbc_cra_prod'  END 
		

		DECLARE @Deal_Type nvarchar(10)						SET @Deal_Type					= (CASE WHEN LEFT(@Account_Type,7) = 'ACCRUAL' THEN 'ACCRUAL' ELSE 'STATEMENT' END) --Accrual STATEMENT
		SET @Contract_ID			= REPLACE(REPLACE(@Contract_ID,', ',',') ,' ,',',') 
		SET @Product_ID				= REPLACE(@Product_ID,' ','') 
		SET @Contributor_ID			= REPLACE(@Contributor_ID,' ','') 
		DECLARE @Product_ID_Filter varchar(1000)		SET @Product_ID_Filter			= ''
		DECLARE @SQL nvarchar(max)
		DECLARE @Vendor_Paying_Account nvarchar(60)		SET @Vendor_Paying_Account		= 'Payment Recipient Export ID'
		DECLARE @Contract_Filter nvarchar(4000)			SET @Contract_Filter			= ''
		IF (@Product_ID			&lt;&gt; 'All')						BEGIN	SET @Product_ID					= '('''+REPLACE(@Product_ID,',',''',''')+''')'  		END
		IF (@Product_ID			&lt;&gt; 'All')						BEGIN	SET @Product_ID_Filter			= 'INNER JOIN  #c_udkey_1_Temp cu1 on cu1.udkey_1_sid = xdcr.udkey_1_sid '			END
		
	
