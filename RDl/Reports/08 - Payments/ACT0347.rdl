		IF OBJECT_ID('tempdb..#x_contract') IS NOT NULL  DROP TABLE #x_contract 								CREATE TABLE #x_contract (contract_sid int, Contract_id nvarchar(30), Contract_id_Export nvarchar(30),  Contract_Description nvarchar(30), status_sid int, admin_class_sid int , Multi_Particpant nvarchar(1) DEFAULT 'N', Contract_Status nvarchar(30),Admin_class nvarchar(30),Producers_Gross nvarchar(1) DEFAULT 'N',Include_Payee_n_Identifier nvarchar(3), keyword nvarchar(30)) 
		IF OBJECT_ID('tempdb..#C_Contact') IS NOT NULL DROP TABLE #C_Contact									CREATE TABLE #C_Contact (contract_sid int ,Contributor_sid int , Contributor_ID nvarchar(30), Contributor_Name nvarchar(30), Contributor_Order nvarchar(3), Contributor_recipient_group_sid int)
		IF OBJECT_ID('tempdb..#Identifier') IS NOT NULL DROP TABLE #Identifier									CREATE TABLE #Identifier (contract_sid int ,  Identifier_sid int,  Identifier_Order int,  Identifier nvarchar(30),  Identifier_ID nvarchar(30),  Identifier_Name nvarchar(30))
		IF OBJECT_ID('tempdb..#Payee') IS NOT NULL DROP TABLE #Payee											CREATE TABLE #Payee (contract_sid int ,  Payee_sid int,  Payee_Order int,   Payee_ID nvarchar(30),  Payee_Name nvarchar(30))
		IF OBJECT_ID('tempdb..#Account') IS NOT NULL DROP TABLE #Account										CREATE TABLE #Account (udkey_2_sid int,udkey_3_sid int, Amount nvarchar(500))
		IF OBJECT_ID('tempdb..#templates') IS NOT NULL BEGIN DROP TABLE #templates		end						CREATE TABLE #templates (deal_sid int, royalty_period_sid int, preparer_sid int,Preparer nvarchar(30), template_sid int, template_status_sid int, Date_due date, Date_Run date, Date_Approved date, Approval_Comment nvarchar(30), Royalty_Period_Date date, Royalty_Period nvarchar(30), contract_sid int)
		IF OBJECT_ID('tempdb..#x_keyword') IS NOT NULL DROP TABLE #x_keyword									CREATE TABLE #x_keyword (contract_sid int, keyword nvarchar(30) )
		IF OBJECT_ID('tempdb..#Product_Filter') IS NOT NULL DROP TABLE #Product_Filter							CREATE TABLE #Product_Filter (udkey_1_sid int )
		IF OBJECT_ID('tempdb..#x_contract_Product_filter') IS NOT NULL DROP TABLE #x_contract_Product_filter	CREATE TABLE #x_contract_Product_filter (contract_sid int,udkey_1_sid int,udkey_1_group_sid int)
	 		
	/**************************************************************************************************************************************************************************************/
	/*********************** Contract Filter **********************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
		
		DECLARE  @Contract_ID_Filter nvarchar(500)
		IF (@Contract_ID &lt;&gt; 'All') BEGIN   SET @Contract_ID_Filter =  ' AND contract_id IN ('''+REPLACE(REPLACE(@Contract_ID,',',''','''),', ',',')+''')' END
		IF (@Contract_ID = 'All') BEGIN   SET @Contract_ID_Filter =  '' END

	

		SET @SQL =
		'
		INSERT INTO #x_contract (contract_sid, contract_id,Contract_id_Export,  Contract_Description, status_sid, admin_class_sid,Include_Payee_n_Identifier,keyword)
		select contract_sid, LEFT(contract_id,30) as contract_id ,LEFT(contract_id,30) as Contract_id_Export ,REPLACE(LEFT(descr,30),'','','''') as Contract_Description, status_sid, admin_class_sid, '''+@Include_Payee_n_Identifier+''' as Include_Payee_n_Identifier, '''+@keyword+''' as keyword
		from x_contract
		where contract_sid = original_contract_sid
		'+@Contract_ID_Filter+'
		'
		exec (@SQL)

		SET @SQL=
			'
					SET DATEFORMAT dmy;
					;WITH CTE_Get_Deal_Data as
					(
					select xtjqh.deal_sid , xtjqh.processing_cutoff_period_sid as royalty_period_sid ,xtjqh.preparer_sid as preparer_sid,xtjqh.template_sid , xtjqh.status_sid as template_status_sid, xtjqh.due_datetime as Date_due    
					'+@Approved_Or_Not_Filter+' 
					INNER JOIN #templates t on t.template_sid = xtjqh.template_sid
					'+@Method_Filter+'
					'+@preparer_Filter+'
					)
					INSERT INTO #x_contract (deal_sid,royalty_period_sid, preparer_sid,Date_due,Date_Run, Date_Approved ,  approval_comment , Contract_Id, admin_class_sid,  Contract_Description  ,status_sid 	,contract_sid	,template_sid,template_status_sid)
					select 
					xtjqh.deal_sid							AS deal_sid
					,xtjqh.royalty_period_sid				AS royalty_period_sid
					,xtjqh.preparer_sid						AS preparer_sid
					,xtjqh.Date_due							AS Date_due
					,xtjqh.Date_Run							AS Date_Run
					,xtjqh.Date_Approved					AS Date_Approved
					,LEFT(xtjqh.approval_comment,60)		AS approval_comment
					,LEFT(xc.Contract_Id,30)				AS Contract_Id
					,xc.admin_class_sid						AS admin_class_sid
					,LEFT(xc.descr,60)						as Contract_Description
					,xc.status_sid							AS status_sid
					,xc.contract_sid 						AS contract_sid
					,xtjqh.template_sid 
					,xtjqh.template_status_sid
					from CTE_Get_Deal_Data xtjqh
					INNER JOIN		x_deal xd with (nolock) on xd.deal_sid =xtjqh.deal_sid
					INNER JOIN		x_contract xc with (nolock) on xc.contract_sid = xd.contract_sid
					'+@c_contact_Filter+'
					'+@Product_Scope_Filter+'
					where xd.section_group_sid '+@MCPS_Filter+'
					'+@Contract_Filter+'
			'
			EXEC (@SQL)


		CREATE CLUSTERED INDEX IX_x_contract_Temp_GH ON [dbo].[#x_contract] ([contract_sid])	-- CREATE NONCLUSTERED INDEX IX_x_contract_udf_lookup_GH ON [dbo].[x_contract_udf_lookup] ([udf_sid],[user_contact_sid])INCLUDE ([contract_sid],[sort_order_nbr])

			SET @SQL =
	'	
	
		UPDATE #x_contract SET multi_Particpant = ''Y''
		--select multi_Particpant,*
		FROM #x_contract
		where admin_class_sid IN (13,23) 
			
		UPDATE #x_contract
		SET Contract_Status = cs.status_id
		from			#x_contract xc
		LEFT JOIN		'+@Server_n_Database+'.dbo.c_status	cs on cs.status_sid = xc.status_sid
		
		UPDATE #x_contract
		SET admin_class = xa.admin_class_id
		from #x_contract xc
		LEFT JOIN	'+@Server_n_Database+'.dbo.x_admin_class xa   on xa.admin_class_sid = xc.admin_class_sid
		'
		EXEC (@SQL)

		--select * from #x_contract

		DELETE FROM #x_contract where Contract_Status IN ('EXPIRED','MODEL')
		IF( @Account_Type = 'ACCRUAL-MCPS' AND @Contract_ID = 'All') BEGIN DELETE from #x_contract where Contract_id NOT LIKE 'MCPS%' SET @Contract_ID = 'MCPS' END


	/**************************************************************************************************************************************************************************************/
	/*********************** Temaplate &amp; Preparer Filter **********************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/

			DECLARE @Template_Type_Input_Filter nvarchar(500)
			DECLARE @Template_Type_Input_Filter2 nvarchar(500)
			DECLARE @Method_Date_Type nvarchar(30)
			DECLARE @template_Where nvarchar(1000)  SET @template_Where = ''
			DECLARE @Period_End nvarchar(30)		SET @Period_End =''
			IF (@Deal_Type = 'STATEMENT')	BEGIN SET @Template_Type_Input_Filter = 'Stmt'  SET @Template_Type_Input_Filter2 = 'Statement' END
			IF (@Deal_Type &lt;&gt; 'STATEMENT')	BEGIN SET @Template_Type_Input_Filter = 'Acc' SET @Template_Type_Input_Filter2 = 'Acc' END
			IF (@Method IN ( 'Last_Paid_Statement','Last_Paid_Statement')) BEGIN SET @Template_Type_Input_Filter = 'Stmt'  SET @Template_Type_Input_Filter2 = 'Statement' END
	
					/**************************************************************************************************************************************************************************************/
					/*********************** Temaplate Main Filters ***************************************************************************************************************************************/
					/**************************************************************************************************************************************************************************************/
	
					IF (@Method IN ('Period_end','Date_Due','Date_Approved','Date_Run'))
					BEGIN
						IF (@preparer &lt;&gt; 'All')			BEGIN SET @Preparer_Filter = ' AND xtjqh.preparer_sid =  CAST((select [user_sid] from x_user where formatted_name ='''+@preparer+''') as nvarchar(10))'  END
						IF(@Method = 'Date_Due')		BEGIN SET @Method_Date_Type  =  'xtjqh.due_datetime'			SET @template_Where = 'CAST('+@Method_Date_Type+' as datetime) &gt;= '''+CAST(@due_dateStart as nvarchar(100)) +'''  AND '+@Method_Date_Type+' &lt;= '''+ CAST(@due_dateEnd as nvarchar(100))+''' and xtjqh.status_sid  IN (16 ,5)' END
						IF(@Method = 'Date_Run')		BEGIN SET @Method_Date_Type  =  'xtjqh.actual_run_date'			SET @template_Where = 'CAST('+@Method_Date_Type+' as datetime) &gt;= '''+CAST(@due_dateStart as nvarchar(100)) +'''  AND '+@Method_Date_Type+' &lt;= '''+ CAST(@due_dateEnd as nvarchar(100))+''' and xtjqh.status_sid  IN (16 ,5)' END
						IF(@Method = 'Date_Approved')	BEGIN SET @Method_Date_Type  =  'xa.approved_datetime'			SET @template_Where = 'CAST('+@Method_Date_Type+' as datetime) &gt;= '''+CAST(@due_dateStart as nvarchar(100)) +'''  AND '+@Method_Date_Type+' &lt;= '''+ CAST(@due_dateEnd as nvarchar(100))+''' and xtjqh.status_sid  IN (16 ,5)' END
						IF(@Method = 'Period_end')		BEGIN SET @Method_Date_Type  =  'xtjqh.due_datetime'			SET @Period_End = CAST((select top 1 CAST(period_sid as nvarchar(10)) from x_period where period_ending_date &gt; CAST( CAST(LEFT(@due_dateEnd,4) as nvarchar(4)) +'-'+CAST(DATEPART ( MM , @due_dateEnd )as nvarchar(2)) +'-15' as datetime) 	and period_ending_date &lt; GETDATE() order by period_ending_date) as nvarchar(30))
															  IF OBJECT_ID('tempdb..#Period_sid_Helper') IS NOT NULL DROP TABLE #Period_sid_Helper		select distinct  due_datetime INTO #Period_sid_Helper from x_template_job_queue_hist where processing_cutoff_period_sid  = @Period_End
															  SET @due_dateStart		= (select top 1 due_datetime-1 from #Period_sid_Helper order by due_datetime asc)
															  SET @due_dateEnd		= (select top 1 due_datetime+1 from #Period_sid_Helper order by due_datetime desc)
															  SET @template_Where = 'CAST('+@Method_Date_Type+' as datetime) &gt;= '''+CAST(@due_dateStart as nvarchar(100)) +'''  AND '+@Method_Date_Type+' &lt;= '''+ CAST(@due_dateEnd as nvarchar(100))+''' AND xtjqh.processing_cutoff_period_sid  = '+@Period_End+' and xtjqh.status_sid  IN (16 ,5)'
														END
		
						IF (@Approved_Or_Not  = 'Approved') 
						BEGIN
							SET @Template_Filter = 
							'
							INSERT INTO #templates (deal_sid, royalty_period_sid, preparer_sid,template_sid, template_status_sid, Date_due, Date_Run, Date_Approved, Approval_Comment )
							select  xtjqh.deal_sid , xtjqh.processing_cutoff_period_sid as royalty_period_sid ,xtjqh.preparer_sid as preparer_sid,xtjqh.template_sid , xtjqh.status_sid as template_status_sid, CAST(xtjqh.due_datetime as date) as Date_due  ,CAST(xtjqh.actual_run_date as date) as Date_Run, CAST(xa.approved_datetime as date) as Date_Approved, REPLACE(LEFT(xa.comment,30),'','','''') as approval_comment 
							from   '+@Server_n_Database+'.dbo.x_template_job_queue_hist xtjqh  
							INNER JOIN  '+@Server_n_Database+'.dbo.x_approval  xa on xa.approval_sid = xtjqh.approval_sid  
							INNER JOIN CTE_Templates t on t.template_sid = xtjqh.template_sid
							where  '+@template_Where+'
							'
						END

						IF ( @Approved_Or_Not = 'Not_Approved') 
						BEGIN
							SET @Template_Filter = 
							'
							INSERT INTO #templates (deal_sid, royalty_period_sid, preparer_sid,template_sid, template_status_sid, Date_due )
							select  xtjqh.deal_sid , xtjqh.processing_cutoff_period_sid as royalty_period_sid ,xtjqh.preparer_sid as preparer_sid,xtjqh.template_sid , xtjqh.status_sid as template_status_sid, CAST(xtjqh.due_datetime as date) as Date_due  
							 from   '+@Server_n_Database+'.dbo.x_template_job_queue xtjqh 
								INNER JOIN CTE_Templates t on t.template_sid = xtjqh.template_sid
							where  '+@template_Where+'
							'+@Preparer_Filter+'
							'
						END

		
					END
				
					/**************************************************************************************************************************************************************************************/
					/*********************** Temaplate Date_Last_Approved Filters *************************************************************************************************************************/
					/**************************************************************************************************************************************************************************************/

					IF (@Method IN( 'Last_Approved_Statement','Last_Paid_Statement'))
					BEGIN	
						IF( @due_dateStart &lt; (GETDATE()-365*2)) BEGIN SET @due_dateStart = (GETDATE()-365*2)  END
						SET @Method_Date_Type  =  'xa.approved_datetime'			SET @template_Where = 'CAST('+@Method_Date_Type+' as datetime) &gt;= '''+CAST(@due_dateStart as nvarchar(100)) +'''  AND '+@Method_Date_Type+' &lt;= '''+ CAST(@due_dateEnd as nvarchar(100))+''' and xtjqh.status_sid  IN (16 ,5)' 
						
						SET @Template_Filter = 
							'
							,CTE_GET_Last_Approved AS
							(
							select  xtjqh.deal_sid , xtjqh.processing_cutoff_period_sid as royalty_period_sid ,xtjqh.preparer_sid as preparer_sid,xtjqh.template_sid , xtjqh.status_sid as template_status_sid, CAST(xtjqh.due_datetime as date) as Date_due  ,CAST(xtjqh.actual_run_date as date) as Date_Run, CAST(xa.approved_datetime as date) as Date_Approved, REPLACE(LEFT(xa.comment,30),'','','''') as approval_comment 
							, ROW_NUMBER() OVER (PARTITION BY xtjqh.deal_sid ORDER BY xa.approved_datetime desc) AS Roww
							from   '+@Server_n_Database+'.dbo.x_template_job_queue_hist xtjqh  
							INNER JOIN  '+@Server_n_Database+'.dbo.x_approval  xa on xa.approval_sid = xtjqh.approval_sid  
							INNER JOIN CTE_Templates t on t.template_sid = xtjqh.template_sid
							where  '+@template_Where+'
							--and xtjqh.deal_sid = 1970244398
							)
		
							INSERT INTO #templates (deal_sid, royalty_period_sid, preparer_sid,template_sid, template_status_sid, Date_due, Date_Run, Date_Approved, Approval_Comment )
							select deal_sid, royalty_period_sid, preparer_sid,template_sid, template_status_sid, Date_due, Date_Run, Date_Approved, Approval_Comment
							from CTE_GET_Last_Approved
							where Roww = 1
							'

							SET @Output_Method = 'Contract_Summary'
					END

					/**************************************************************************************************************************************************************************************/
					/*********************** Temaplate Last_Paid_Statement Filters *************************************************************************************************************************/
					/**************************************************************************************************************************************************************************************/

					IF (@Method = 'Last_Paid_Statement')
					BEGIN	
							SET @Template_Filter = REPLACE(@Template_Filter,'where Roww = 1','')
					end


					/**************************************************************************************************************************************************************************************/
					/*********************** Temaplate RUN Filters ***************************************************************************************************************************************/
					/**************************************************************************************************************************************************************************************/

					-- Run Template script
					SET @SQL =
						'
						SET DATEFORMAT ymd;
						;WITH CTE_Templates AS
						(
						select  d.template_sid		
						from '+@Server_n_Database+'.dbo.c_template_group_hdr  h   
						left join '+@Server_n_Database+'.dbo.c_template_group_dtl d on d.template_group_sid = h.template_group_sid
						where h.name LIKE ''%'+@Template_Type_Input_Filter+'%''
						UNION 
						select  d.template_sid		
						from '+@Server_n_Database+'.dbo.c_template_group_hdr  h   
						left join '+@Server_n_Database+'.dbo.c_template_group_dtl d on d.template_group_sid = h.template_group_sid
						where h.name LIKE ''%'+@Template_Type_Input_Filter2+'%''
	
						)
						'+@Template_Filter+'
						'
						exec (@SQL)

						CREATE CLUSTERED INDEX IX_Template_GH ON #templates (deal_sid, royalty_period_sid) 					
						SET @SQL =
						'	
						UPDATE #templates
						SET Preparer =  LEFT(xu.formatted_name,30)
						from #templates xc
						LEFT JOIN		'+@Server_n_Database+'.dbo.x_user xu on xc.preparer_sid = xu.[user_sid]

						UPDATE #templates
						SET Royalty_Period = xp.period_id,  Royalty_Period_Date = CAST(xp.period_ending_date as date)
						from #templates xc
						LEFT JOIN	'+@Server_n_Database+'.dbo.x_period  xp on xc.royalty_period_sid = xp.period_sid

						UPDATE #templates
						SET contract_sid = xd.contract_sid
						from		#templates t
						INNER JOIN	'+@Server_n_Database+'.dbo.x_deal xd on xd.deal_sid = t.deal_sid
						'
						EXEC (@SQL)
						DELETE FROM #templates where contract_sid NOT IN (select contract_sid from #x_contract)

					
