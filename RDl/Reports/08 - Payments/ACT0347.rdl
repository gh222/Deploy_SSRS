	/**************************************************************************************************************************************************************************************/
	/*********************** Deal Filter *******************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/
		
	DELETE FROM #templates where deal_sid NOT IN(
												select xd.deal_sid
												from x_deal xd
												INNER JOIN #templates t on t.deal_sid = xd.deal_sid
												INNER JOIN #x_contract xc on xc.contract_sid = xd.contract_sid
												)

	/**************************************************************************************************************************************************************************************/
	/*********************** Specific to Report Filter *******************************************************************************************************************************************/
	/**************************************************************************************************************************************************************************************/

		IF (@Approved_Or_Not	= 'Approved')					BEGIN	SET @Approved_Or_Not_Filter		=  ' ,xtjqh.actual_run_date as Date_Run, xa.approved_datetime as Date_Approved, REPLACE(xa.comment,'','','''') as approval_comment from   dbo.x_template_job_queue_hist xtjqh INNER JOIN  dbo.x_approval  xa on xa.approval_sid = xtjqh.approval_sid'  END
		IF (@Approved_Or_Not	= 'Not_Approved')				BEGIN	SET @Approved_Or_Not_Filter		=  ' ,null as Date_Run, NULL as Date_Approved,NULL as approval_comment from   dbo.x_template_job_queue xtjqh '								END
		IF (@Method				= 'Date_Due')					BEGIN	SET @Method_Filter				= ' AND xtjqh.due_datetime &gt;= '''+ CONVERT(VARCHAR(10),@due_dateStart, 104) +'''  AND xtjqh.due_datetime &lt;= '''+ CONVERT(VARCHAR(10),@due_dateEnd, 104) +''' and xtjqh.status_sid  IN (16 ,5) '  END  --and xtjqh.status_sid IN (16 ,5)
		IF (@Method				= 'Date_Approved')				BEGIN	SET @Method_Filter				= ' AND xa.approved_datetime &gt;= '''+ CONVERT(VARCHAR(10),@due_dateStart, 104) +'''  AND xa.approved_datetime &lt;= '''+ CONVERT(VARCHAR(10),@due_dateEnd, 104) +'''  and xtjqh.status_sid  IN (16 ,5) '  END 
		IF (@Method				= 'Date_Run')					BEGIN	SET @Method_Filter				= ' AND xtjqh.actual_run_date &gt;= '''+ CONVERT(VARCHAR(10),@due_dateStart, 104) +'''  AND xtjqh.actual_run_date &lt;= '''+ CONVERT(VARCHAR(10),@due_dateEnd, 104) +''' and xtjqh.status_sid  IN (16 ,5) '  END 
		IF (@preparer			&lt;&gt; 'All')						BEGIN	SET @preparer_Filter			= ' AND xtjqh.preparer_sid = ' + CAST((select [user_sid] from x_user where formatted_name = @preparer) as nvarchar(10))			END
		IF (@preparer			= 'All')						BEGIN	SET @preparer_Filter			= ' '			END
		IF (@Contract_ID		LIKE 'MCPS%')					BEGIN	SET @MCPS_Filter = 'is not null' END
		IF (@Contract_ID		 NOT LIKE 'MCPS%')				BEGIN	SET @MCPS_Filter = 'is null'  END
		IF (@Contract_ID		&lt;&gt; 'All')						BEGIN   SET @Contract_Filter			= ' AND xc.contract_id IN ('''+REPLACE(@Contract_ID,',',''',''')+''')' END 	
	
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/****************************** END REPORT SPECIFIC FILTERS ***************************************************************************************************************************************************************/
