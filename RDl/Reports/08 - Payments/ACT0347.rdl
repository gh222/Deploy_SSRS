					INNER JOIN #x_contract xc on xc.contract_sid = xck.contract_sid
					where xck.keyword_sid = (	select keyword_sid from WGB01DB7047.bbc_cra_prod.dbo.c_keyword where descr = '''+@Keyword+''')
		
					'
					SET @Keyword = ' INNER JOIN #keyword_contracts kc on kc.contract_sid = xd.contract_sid '
			END
