						INSERT INTO #x_contract_Product_filter (contract_sid,udkey_1_sid )
						select xc.contract_sid, cte.udkey_1_sid from #x_contract xc,CTE_Products cte
						where  xc.Contract_id LIKE 'MCPS%'
				END

			CREATE CLUSTERED INDEX IX_x_contract_Product_filter_GH ON #x_contract_Product_filter (contract_sid) 

			DELETE FROM #x_contract where contract_sid NOT IN (select distinct contract_sid from #x_contract_Product_filter)
			DELETE FROM #templates where contract_sid NOT IN (select distinct contract_sid from #x_contract_Product_filter)

			SET @Product_Scope_Filter = ' INNER JOIN #x_contract_Product_filter xcpf on xcpf.udkey_1_sid = xdcr.udkey_1_sid '
