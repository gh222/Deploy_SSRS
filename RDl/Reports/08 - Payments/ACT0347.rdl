					INNER JOIN '+@Server_n_Database+'.dbo.c_udkey_1_group_dtl cu1d on cu1d.udkey_1_group_sid = xcr.udkey_1_group_sid
					INNER JOIN #Product_Filter pf on pf.udkey_1_sid = cu1d.udkey_1_sid
