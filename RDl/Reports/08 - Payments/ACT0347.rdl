		DECLARE @c_contact_Filter nvarchar(1000)		SET @c_contact_Filter = ''		DECLARE @c_contact_Table nvarchar(1000)
		DECLARE @Account_Filter nvarchar(1000)			SET @Account_Filter = ''		DECLARE @Account_Table nvarchar(1000) 
		DECLARE @Template_Filter nvarchar(2000)			SET @Template_Filter = ''		DECLARE @Template_Table nvarchar(1500)
		DECLARE @Deal_Filter nvarchar(1000)				SET @Deal_Filter = ''			DECLARE @Deal_Table nvarchar(1000)
		DECLARE @Keyword_Filter nvarchar(1000)			SET @Keyword_Filter = ''		DECLARE @Keyword_Table nvarchar(1000)
		declare @Product_Scope_Filter nvarchar(1000)	SET @Product_Scope_Filter = ''	DECLARE @Product_Scope_Table nvarchar(1000) 

		DECLARE @MCPS_Filter nvarchar(100)
		DECLARE @c_contract_Table nvarchar(2000)
		DECLARE @Approved_Or_Not_Filter varchar(1000)	
		DECLARE @Method_Filter nvarchar(1000)
		DECLARE @preparer_Filter varchar(400)			SET @preparer_Filter =''	
		DECLARE @c_contract_Create nvarchar(500)
