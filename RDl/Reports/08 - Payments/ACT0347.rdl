--select * from #account
--select * from #x_contract
--select @Contract_ID
--select * from #templates 

		IF OBJECT_ID('tempdb..#Results') IS NOT NULL DROP TABLE #Results
		CREATE TABLE #Results (deal_sid int,contract_sid int, user_contact_sid int,royalty_period_sid int,sales_period_sid int,amount decimal(18,2),qty int,udkey_1_sid int,udkey_2_sid int,udkey_3_sid int,udkey_4_sid int,udkey_5_sid int,udkey_6_sid int,udkey_7_sid int,udkey_8_sid int,udkey_9_sid int,udkey_10_sid int,udkey_12_sid int,udkey_13_sid int, udkey_14_sid int,udkey_16_sid int,udkey_17_sid int,LOTCAPS_Resolved_Num int ,Price decimal(18,2) , Rate decimal(18,4))
		
	
		/**************************************************************************************************************************************************************************************/
		/*********************** When Processing Many Deals Such as Accrual *********************************************************************************************************************************************/
		/**************************************************************************************************************************************************************************************/
	
		IF (@Contract_ID =  'All' AND LEFT(@Account_Type,7)	= 'ACCRUAL'	)
		BEGIN
			--RAISERROR( 'Processing Results All Accrual',0,1) WITH NOWAIT
			--IF OBJECT_ID('tempdb..#Results') IS NOT NULL DROP TABLE #Results

			SET @SQL = 
			'
			;WITH CTE_Period AS
			(
			select distinct royalty_period_sid from #templates --order by royalty_period_sid
			)
			, CTE_Get_Results AS
			(
			select xdcr.deal_sid, xdcr.user_contact_sid	,xdcr.period_sid as royalty_period_sid, xdcr.actual_period_sid as sales_period_sid, xdcr.amount as amount , xdcr.qty ,xdcr.udkey_1_sid,xdcr.udkey_2_sid,xdcr.udkey_3_sid,xdcr.udkey_4_sid,xdcr.udkey_5_sid,xdcr.udkey_6_sid,xdcr.udkey_7_sid,xdcr.udkey_8_sid,xdcr.udkey_9_sid,xdcr.udkey_10_sid,xdcr.udkey_12_sid,xdcr.udkey_13_sid,xdcr.udkey_14_sid,xdcr.udkey_16_sid,xdcr.udkey_17_sid
			from			x_deal_calc_result	xdcr with (nolock)
			INNER JOIN		CTE_Period		p on p.royalty_period_sid = xdcr.period_sid
			)
			, CTE_Account AS
			(
			select distinct udkey_2_sid from #Account-- order by udkey_2_sid
			)
			INSERT INTO #Results (contract_sid,deal_sid ,user_contact_sid ,royalty_period_sid ,sales_period_sid , amount ,qty ,udkey_1_sid ,udkey_2_sid ,udkey_3_sid ,udkey_4_sid ,udkey_5_sid ,udkey_6_sid ,udkey_7_sid ,udkey_8_sid ,udkey_9_sid ,udkey_10_sid ,udkey_12_sid ,udkey_13_sid, udkey_14_sid ,udkey_16_sid ,udkey_17_sid,Price, Rate)		
			select 999999999 as contract_sid, xdcr.deal_sid, xdcr.user_contact_sid	,xdcr.royalty_period_sid as royalty_period_sid, xdcr.sales_period_sid as sales_period_sid, xdcr.amount as amount , xdcr.qty ,xdcr.udkey_1_sid,xdcr.udkey_2_sid,xdcr.udkey_3_sid,xdcr.udkey_4_sid,xdcr.udkey_5_sid,xdcr.udkey_6_sid,xdcr.udkey_7_sid,xdcr.udkey_8_sid,xdcr.udkey_9_sid,xdcr.udkey_10_sid,xdcr.udkey_12_sid,xdcr.udkey_13_sid,xdcr.udkey_14_sid,xdcr.udkey_16_sid,xdcr.udkey_17_sid,xdcr.price_point as Price, xdcr.user_rate AS Rate
			--INTO #Results
			from		CTE_Get_Results		xdcr
			INNER JOIN	CTE_Account		a	ON a.udkey_2_sid = xdcr.udkey_2_sid
			'
			EXEC (@SQL)
			--print (@SQL)

			-- Delete Trans type not needed as no index on transtype
			DECLARE @trans int SET @trans = (select distinct udkey_3_sid from #Account) 
			DELETE FROM #Results where udkey_3_sid &lt;&gt; @trans
			-- Delete not needed deals
			
			DELETE from #Results where deal_sid NOT IN (select deal_sid from #templates)

		--	select * from #Results where deal_sid = 1970245642
		--	select * from #templates  where deal_sid = 1970245642

			UPDATE #Results
			set contract_sid = t.contract_sid 
			--select r.contract_sid, t.contract_sid 
			from #Results r
			INNER JOIN #templates t on t.deal_sid = r.deal_sid 
		END

		/**************************************************************************************************************************************************************************************/
		/*********************** When Processing Limited Deals Such as Accrual *********************************************************************************************************************************************/
		/**************************************************************************************************************************************************************************************/
	
		ELSE
		--IF (@Contract_ID &lt;&gt;  'All'	)
		BEGIN
		--RAISERROR( 'Processing Results Limited Deals',0,1) WITH NOWAIT
					
			SET @SQL =
			'
			INSERT INTO #Results (deal_sid ,user_contact_sid ,royalty_period_sid ,sales_period_sid , amount ,qty ,udkey_1_sid ,udkey_2_sid ,udkey_3_sid ,udkey_4_sid ,udkey_5_sid ,udkey_6_sid ,udkey_7_sid ,udkey_8_sid ,udkey_9_sid ,udkey_10_sid ,udkey_12_sid ,udkey_14_sid ,udkey_16_sid ,udkey_17_sid,contract_sid,Price,Rate)		
			select xdcr.deal_sid, xdcr.user_contact_sid	,xdcr.period_sid as royalty_period_sid, xdcr.actual_period_sid as sales_period_sid, xdcr.amount as amount , xdcr.qty ,xdcr.udkey_1_sid,xdcr.udkey_2_sid,xdcr.udkey_3_sid,xdcr.udkey_4_sid,xdcr.udkey_5_sid,xdcr.udkey_6_sid,xdcr.udkey_7_sid,xdcr.udkey_8_sid,xdcr.udkey_9_sid,xdcr.udkey_10_sid,xdcr.udkey_12_sid,xdcr.udkey_14_sid,xdcr.udkey_16_sid,xdcr.udkey_17_sid, t.contract_sid,xdcr.price_point as Price, xdcr.user_rate*100 AS Rate
			from			x_deal_calc_result xdcr with (nolock)
			INNER JOIN #Account a on a.udkey_2_sid = xdcr.udkey_2_sid and a.udkey_3_sid = xdcr.udkey_3_sid
			INNER JOIN #templates t on t.deal_sid = xdcr.deal_sid and xdcr.period_sid = t.royalty_period_sid
			'+@Product_Scope_Filter+'

			'
			EXEC (@SQL)
		--	print (@SQL)

		END
	
	
			/**************************************************************************************************************************************************************************************/
			/*********************** Non Multi Contrib updates *********************************************************************************************************************************************/
			/**************************************************************************************************************************************************************************************/
	


			-- Update Non Multi
			UPDATE #Results
			SET user_contact_sid = cc.Contributor_sid
			--  select r.user_contact_sid, cc.Contributor_sid
			from #Results r
			LEFT JOIN #c_contact cc on cc.contract_sid = r.contract_sid
			where r.user_contact_sid = 0 AND cc.Contributor_sid IS NOT NULL 

			IF (@Contributor_Name &lt;&gt; 'All' OR @Contributor_ID &lt;&gt; 'All')
			BEGIN
					DELETE FROM #Results where user_contact_sid NOT IN (select  contributor_sid from #c_contact)
			END
		
		/*****************************************************************************************************/
		/*****************************************************************************************************/
		/****************************** Lotcsap  *************************************************************/
		/*****************************************************************************************************/
		/*****************************************************************************************************/
			
			IF OBJECT_ID('tempdb..#Lotcsap') IS NOT NULL DROP TABLE #Lotcsap
			CREATE TABLE #Lotcsap (contract_sid int, seq_nbr int, sort_order_nbr int, contact_sid int, LOTCAPS_Resolved_Num int)

			IF (@Include_Payee_n_Identifier	= 'YES'	AND @Output_Method		= 'Contract_Detail')
			BEGIN

					insert into #Lotcsap (contract_sid , seq_nbr , sort_order_nbr , contact_sid , LOTCAPS_Resolved_Num )
					select xcu.contract_sid,  xcu.seq_nbr, xcu.sort_order_nbr, xcu.user_contact_sid as contact_sid, xcu.value as LOTCAPS_Resolved_Num
					from x_contract_udf_lookup xcu
					INNER JOIN		#x_contract				xc on xc.contract_sid = xcu.contract_sid
					where  udf_sid IN (151,212)
					UNION ALL
					select xcu.contract_sid,  0 as seq_nr, 0 as sort_order_nbr, cc.contact_sid as contact_sid,xcu.udf_value as LOTCAPS_Resolved_Num
					from x_contract_udf xcu
					INNER JOIN		#x_contract				xc on xc.contract_sid = xcu.contract_sid
					INNER JOIN		x_contract_participant	xcp on xcp.contract_sid = xc.contract_sid
					INNER JOIN		c_contact				cc on cc.contact_sid = xcp.contact_sid
					where udf_sid IN (151,212) and udf_value IS NOT NULL
					
					UPDATE #Results
					SET LOTCAPS_Resolved_Num = l.LOTCAPS_Resolved_Num
				--	select xcu.contact_sid, l.LOTCAPS_Resolved_Num
					from #Results xcu
					INNER JOIN #Lotcsap l on l.contract_sid = xcu.contract_sid and xcu.user_contact_sid = l.contact_sid
					where l.LOTCAPS_Resolved_Num IS NOT NULL
			END

	
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/****************************** END CALCAULTIONS ***************************************************************************************************************************************************************/
/************************************** ********************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/


/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/****************************** START ACCOUNT Manipulation  ***************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
		
			IF (@Account_Type = 'Reserves'			AND @TransType = 'ITD')
			BEGIN
					--DECLARE @Amount nvarchar(500) SET 	@Amount = (select distinct amount from #Account)
		
					UPDATE #Results
					SET Amount = Amount * -1
					from #Results 
					where udkey_2_sid IN (521,89)
			END

			/*
			select * from c_udkey_2 where udkey_2_sid IN (489,1968681871) descr = 'Net Sales Royalties - Equity Royalty Rates'
			select * from #Account
			select * from #templates
			*/

/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/****************************** FINAL COMBINE START ***************************************************************************************************************************************************************/
/************************************** ********************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/


/**********************************************************************************************************************************************************************************************************/
/*********************** All Method Use this base select clause ***********************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/

			DECLARE @Method_To_Run nvarchar(4000) SET @Method_To_Run = ''
			DECLARE @Main_Top_Select_Clause nvarchar(2000)  
			DECLARE @After_Amount_Clause nvarchar(2000) 
			DECLARE @Final_Select_n_Join_Clause nvarchar(2000) 
			DECLARE @Amount_Clause nvarchar(1000) 
			DECLARE @Contrib_Clause nvarchar(1000)							DECLARE @Contrib_Not_Null_Clause nvarchar(1000) 
			DECLARE @Group_By_Clause nvarchar(1000) 
			DECLARE @Detail_Join_Clause nvarchar(1000) 

			SET @Main_Top_Select_Clause =
			'select 
					r.deal_sid, r.contract_sid, r.user_contact_sid
					,''|'' AS x_0
					,xc.contract_id	,CASE WHEN xc.Include_Payee_n_Identifier = ''NO'' THEN NULL WHEN xc.Multi_Particpant = ''Y'' THEN xc.Contract_id_Export+ISNULL(i.Identifier,'''') ELSE xc.Contract_id_Export  END AS Contract_id_Export 	,xc.Contract_Description,xc.Multi_Particpant, xc.Producers_Gross	,xc.Contract_Status	,xc.Admin_class
					,''|'' AS x_1'

			SET @Contrib_Clause =
								'	
						,cc.Contributor_ID 	,cc.Contributor_Name	 ,cc.Contributor_Order--,cc.Contributor_recipient_group_sid
						,''|'' AS x_2		
						,i.Identifier_ID,	i.Identifier_Name   , i.Identifier_Order	,i.Identifier as Identifier	
						,''|'' AS x_3
						, p.Payee_Order,p.Payee_ID,	p.Payee_Name 
						,''|'' as x_5 	,r.LOTCAPS_Resolved_Num '

			SET @After_Amount_Clause =
			',''|'' AS x_4
			,cu2.udkey_2_id as Account,cu3.udkey_3_id as TransType, xp1.period_id as Royalty_Period, xp2.period_id as  Sales_Period '

			SET @Final_Select_n_Join_Clause =
			'	,t.Preparer,xc.keyword AS Keyword
					, t.Date_due
					,t.Date_Run,t.Date_Approved, CAST(GETDATE() as date) AS Extraction_Date, t.Approval_Comment
					from	#Results	r 	
					LEFT JOIN	c_udkey_2 cu2 ON cu2.udkey_2_sid = r.udkey_2_sid 
					LEFT JOIN	c_udkey_3 cu3 ON cu3.udkey_3_sid = r.udkey_3_sid 
					LEFT JOIN	x_period xp1 on xp1.period_sid = r.royalty_period_sid
					LEFT JOIN	x_period xp2 on xp2.period_sid = r.sales_period_sid
					LEFT JOIN	#Identifier i on i.contract_sid = r.contract_sid AND i.Identifier_sid	 = r.user_contact_sid
					LEFT JOIN	#x_contract xc on xc.contract_sid = r.contract_sid
					LEFT JOIN	#templates t on t.contract_sid = r.contract_sid and t.deal_sid = r.deal_sid and t.royalty_period_sid = r.royalty_period_sid '

			SET @Group_By_Clause =' GROUP BY r.deal_sid, r.contract_sid, r.user_contact_sid,	xc.contract_id, xc.Contract_Description,xc.Multi_Particpant, xc.Producers_Gross	,xc.Contract_Status	,xc.Admin_class	,xc.Include_Payee_n_Identifier,i.Identifier	,xc.Contract_id_Export	,cu2.udkey_2_id ,cu3.udkey_3_id	,t.Date_Due	,t.Preparer,xc.keyword 	, t.Date_due	,t.Date_Run,t.Date_Approved, t.Approval_Comment,		xp1.period_id , xp2.period_id,cc.Contributor_ID 	,cc.Contributor_Name	 ,cc.Contributor_Order,i.Identifier_ID,	i.Identifier_Name   , i.Identifier_Order	,i.Identifier,p.Payee_Order,p.Payee_ID,	p.Payee_Name ,r.LOTCAPS_Resolved_Num'

/**********************************************************************************************************************************************************************************************************/
/*********************** Method_Contract_Summary ***********************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/

		IF (@Output_Method	= 'Contract_Summary')
		BEGIN
					SET @Amount_Clause = ' ,SUM(r.qty) as Units  ,SUM(r.Amount) AS Amount	,''|'' AS x_1_				'
					SET @Final_Select_n_Join_Clause = @Final_Select_n_Join_Clause +
					'
						LEFT JOIN	#Payee p on p.contract_sid = r.contract_sid AND p.payee_sid	 = r.user_contact_sid
						LEFT JOIN	#c_contact cc on cc.contract_sid = r.contract_sid AND r.user_contact_sid = cc.Contributor_sid 
					'
					
		END

/**********************************************************************************************************************************************************************************************************/
/*********************** Contract_Detail ***********************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/

		IF (@Output_Method	= 'Contract_Detail')
		BEGIN
					SET @Amount_Clause = ' ,(r.qty) as Units  ,(r.Amount) AS Amount ,r.Price AS Price, r.Rate AS Rate   ,''|'' AS x_1_ '
					SET @Contrib_Clause =',cu10.udkey_10_id as Currency, CAST(cu1.udkey_1_id as nvarchar(30)) as Product_ID,REPLACE(LEFT(cu1.descr,60),'','','''') as Product_Description, cu4.udkey_4_id as Sales_Type_ID,cu6.udkey_6_id as Distrubtor,cu12.udkey_12_id Department,cu7.udkey_7_id Media,cu14.udkey_14_id as Rights_Territory,cu5.udkey_5_id AS Territory, cu13.udkey_13_id as Media_Code ,cu8.udkey_8_id as Contribution_Type'	+ @Contrib_Clause
					SET @Final_Select_n_Join_Clause = @Final_Select_n_Join_Clause +
						'	LEFT JOIN	c_udkey_1 cu1 ON cu1.udkey_1_sid = r.udkey_1_sid	
							LEFT JOIN	c_udkey_4 cu4 ON cu4.udkey_4_sid = r.udkey_4_sid 
							LEFT JOIN	c_udkey_5 cu5 ON cu5.udkey_5_sid = r.udkey_5_sid
							LEFT JOIN	c_udkey_6 cu6 ON cu6.udkey_6_sid = r.udkey_6_sid	
							LEFT JOIN	c_udkey_7 cu7 ON cu7.udkey_7_sid = r.udkey_7_sid 
							LEFT JOIN	c_udkey_8 cu8 ON cu8.udkey_8_sid = r.udkey_8_sid	
							LEFT JOIN	c_udkey_10 cu10 ON cu10.udkey_10_sid = r.udkey_10_sid	
							LEFT JOIN	c_udkey_12 cu12 ON cu12.udkey_12_sid = r.udkey_12_sid
							LEFT JOIN	c_udkey_13 cu13 ON cu13.udkey_13_sid = r.udkey_13_sid	
							LEFT JOIN	c_udkey_14 cu14 ON cu14.udkey_14_sid = r.udkey_14_sid
							LEFT JOIN	#Payee p on p.contract_sid = r.contract_sid AND p.payee_sid	 = r.user_contact_sid
							LEFT JOIN	#c_contact cc on cc.contract_sid = r.contract_sid AND r.user_contact_sid = cc.Contributor_sid 					'
					SET @Group_By_Clause = ''
		END
/**********************************************************************************************************************************************************************************************************/
/*********************** Contribution Type ***********************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
	IF (@Output_Method	IN ('CONTRACT_BY_CONTRIBUTION','CONTRACT_BY_TERRITORY'))
		BEGIN

					SET @Amount_Clause = ' ,SUM(r.qty) as Units  ,SUM(r.Amount) AS Amount,''|'' AS x_1_ '	
					SET @Final_Select_n_Join_Clause = @Final_Select_n_Join_Clause +
						'	LEFT JOIN	#Payee p on p.contract_sid = r.contract_sid AND p.payee_sid	 = r.user_contact_sid
							LEFT JOIN	#c_contact cc on cc.contract_sid = r.contract_sid AND r.user_contact_sid = cc.Contributor_sid  '

					IF (@Output_Method	= 'CONTRACT_BY_CONTRIBUTION')	BEGIN SET @Contrib_Clause = ',cu8.udkey_8_id as Contribution_Type'	+ @Contrib_Clause END
					IF (@Output_Method	= 'CONTRACT_BY_TERRITORY')		BEGIN SET @Contrib_Clause = ',cu5.udkey_5_id as Territory'		+ @Contrib_Clause END		
						
					IF (@Output_Method	= 'CONTRACT_BY_CONTRIBUTION')	BEGIN SET @Group_By_Clause = ' LEFT JOIN	c_udkey_8 cu8 ON cu8.udkey_8_sid = r.udkey_8_sid ' + @Group_By_Clause + ',cu8.udkey_8_id ' END
					IF (@Output_Method	= 'CONTRACT_BY_TERRITORY')		BEGIN SET @Group_By_Clause = ' LEFT JOIN	c_udkey_5 cu5 ON cu5.udkey_5_sid = r.udkey_5_sid ' + @Group_By_Clause + ',cu5.udkey_5_id ' END
					
	END

	--select * from c_udkey_8 cu8.udkey_8_id as Contribution_Type
	

/**********************************************************************************************************************************************************************************************************/
/*********************** Run Method ***********************************************************************************************************************************************************************************/
/**********************************************************************************************************************************************************************************************************/
	
	SET @Method_To_Run = @Main_Top_Select_Clause + @Amount_Clause +@After_Amount_Clause + @Contrib_Clause +@Final_Select_n_Join_Clause + @Group_By_Clause 
	
	--print (@Method_To_Run)
	exec (@Method_To_Run)
	--IF OBJECT_ID('tempdb..#Results') IS NOT NULL DROP TABLE #Results

</CommandText>
      </Query>
