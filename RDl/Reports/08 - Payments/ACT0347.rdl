					IF (@Method = 'Last_Paid_Statement')
					BEGIN	
							/**************************************************************************************************************************************************************************************/
							/*********************** Last_Paid_Statement Prepare Loop *************************************************************************************************************************/
							/**************************************************************************************************************************************************************************************/
							
						
							-- Make Dummy temp table
							IF OBJECT_ID('tempdb..#templates_Loop') IS NOT NULL DROP TABLE #templates_Loop 
							select * INTO #templates_Loop from #templates
							ALTER TABLE #templates_Loop ADD Last_Paid_Statement bit default 0 UPDATE #templates_Loop SET Last_Paid_Statement = 0
							ALTER TABLE #templates_Loop ADD Contract_ID nvarchar(60)
							UPDATE #templates_Loop SET contract_id = xc.contract_id	from #templates t	inner join x_deal xd on xd.deal_sid = t.deal_sid 	INNER JOIN x_contract xc on xc.contract_sid = xd.contract_sid
							CREATE CLUSTERED INDEX IX_templates_Loop_GH ON #templates_Loop (deal_sid, royalty_period_sid)	

							-- Determine count of contracts
							IF OBJECT_ID('tempdb..#royalty_period_sid') IS NOT NULL DROP TABLE #royalty_period_sid
							CREATE TABLE #royalty_period_sid (royalty_period_sid int , cnt int, royalty_period_date date, Roww int DEFAULT 0)
							CREATE CLUSTERED INDEX IX_royalty_period_sid_GH ON #royalty_period_sid (royalty_period_sid)	
							INSERT INTO #royalty_period_sid (royalty_period_sid, cnt)
							select royalty_period_sid, COUNT(*)from #templates_Loop group by royalty_period_sid
							UPDATE #royalty_period_sid 	SET royalty_period_date = xp.period_ending_date	from #royalty_period_sid r 	INNER JOIN x_period xp on xp.period_sid = r.royalty_period_sid
							;WITH CTE_Row as ( select royalty_period_sid, ROW_NUMBER() OVER (ORDER BY royalty_period_date desc) as Roww	from #royalty_period_sid)
							UPDATE #royalty_period_sid	SET Roww = c.Roww	from CTE_Row c	INNER JOIN #royalty_period_sid r on r.royalty_period_sid = c.royalty_period_sid
							DELETE FROm #royalty_period_sid where cnt  &gt; 3000
		

							-- select * from #templates_Loop
							-- select * from #royalty_period_sid
							
							/**************************************************************************************************************************************************************************************/
							/*********************** Last_Paid_Statement Run Loop *************************************************************************************************************************/
							/**************************************************************************************************************************************************************************************/
					
			
							IF OBJECT_ID('tempdb..#Last_Paid_Statement') IS NOT NULL DROP TABLE #Last_Paid_Statement
							CREATE TABLE #Last_Paid_Statement (deal_sid int, royalty_period_sid int,amount decimal(18,2) )		
							
							DECLARE @Intial_Loops int		SET @Intial_Loops = 1;
							DECLARE @Total_Loops int		SET @Total_Loops = (select count(*) from  	#royalty_period_sid)
							DECLARE @royalty_period_sid int SET @royalty_period_sid = 999
							DECLARE @Total_Counts int		SET @Total_Counts= 0
							DECLARE @Messgae nvarchar(1000) SET @Messgae = 'None'

							WHILE @Intial_Loops &lt;= @Total_Loops
									BEGIN
											TRUNCATE TABLE #Last_Paid_Statement
											SET @royalty_period_sid = (select royalty_period_sid from #royalty_period_sid where Roww = @Intial_Loops)

											RAISERROR ('Before x_deal', 0, 1) WITH NOWAIT
											
											INSERT INTO #Last_Paid_Statement(deal_sid , royalty_period_sid ,amount)	
											select xdcr.deal_sid, xdcr.period_sid as royalty_period_sid, xdcr.amount						
											from x_deal_calc_result xdcr  WITH (NOLOCK)
											INNER JOIN #templates_Loop t on t.royalty_period_sid = xdcr.period_sid AND t.deal_sid = xdcr.deal_sid
											where   xdcr.udkey_2_sid = 433 and xdcr.udkey_3_sid = 1 --and xdcr.amount &lt;&gt; 0 -- royalty_period_sid = 189  and
											and xdcr.period_sid = @royalty_period_sid
											RAISERROR ('After x_deal', 0, 1) WITH NOWAIT

											DELETE FROM #Last_Paid_Statement where amount = 0
											
											
											
											--Update Deals that have a payment
											UPDATE #templates_Loop SET last_paid_statement = 1	/*select t.last_paid_statement*/  from #templates_Loop t	INNER JOIN #Last_Paid_Statement l on l.royalty_period_sid = t.royalty_period_sid and l.deal_sid = t.deal_sid SET @Messgae = ' Payables Found in Current Period: ' + CAST((@@ROWCOUNT) AS nvarchar(10)) RAISERROR (@Messgae, 0, 1) WITH NOWAIT
											-- Delete deals which already have a payment made
											DELETE	FROM #templates_Loop where royalty_period_sid &lt;&gt; @royalty_period_sid AND deal_sid IN (select deal_sid from #templates_Loop t	where t.Last_paid_statement = 1 ) SET @Messgae = ' Payables IN Current Period Deleted from Previous Period : ' + CAST((@@ROWCOUNT) AS nvarchar(10)) RAISERROR (@Messgae, 0, 1) WITH NOWAIT
											
											-- Delete deals in loop period that are not payable
										--	DELETE	FROM #templates_Loop where Last_Paid_Statement = 0 AND royalty_period_sid = @royalty_period_sid SET @Messgae = ' Current Period that is not payable : ' + CAST((@@ROWCOUNT) AS nvarchar(10)) RAISERROR (@Messgae, 0, 1) WITH NOWAIT
											
	
											--print @royalty_period_sid
											SET @Messgae = 'Total Count Of Templates is: ' + CAST((select count(*) from #templates_Loop) AS nvarchar(10)) + ' as Royalty Period: ' +CAST(@royalty_period_sid AS nvarchar(10))
											RAISERROR (@Messgae, 0, 1) WITH NOWAIT
								
											SET @Intial_Loops = @Intial_Loops + 1;
									END;
									
				
					end





