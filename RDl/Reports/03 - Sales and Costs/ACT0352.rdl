  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>Temp_DataSource</DataSourceName>
        <CommandText>-- Sales - Licenced Months Non-PG

;with cte as
(
select 

  cu1.udkey_1_id as Product_ID, CAST(xph.start_user_date as date) AS Licence_Start_Date, CAST(xph.end_user_date as date) AS Licence_End_Date
--, LEFT(cu4.descr,30) as Sales_Type
--, LEFT(cu5.descr,30) as Territory
, LEFT(cu7.descr,30) as Media
, CAST(DATEDIFF(Month,xph.start_user_date,xph.end_user_date)as decimal(18,2)) As Total_Months
, CASE WHEN DATEDIFF(Month,xph.start_user_date,xph.end_user_date) &gt; 84 THEN 'TRUE' ELSE 'FALSE' END AS Clarence_Pymt_Required
, sum(xph.amount)  AS Total_Sales

from			x_posted_history xph
INNER JOIN		c_udkey_1 cu1 on cu1.udkey_1_sid = xph.udkey_1_sid
--INNER JOIN		c_udkey_4 cu4 on cu4.udkey_4_sid = xph.udkey_4_sid
--INNER JOIN		c_udkey_5 cu5 on cu5.udkey_5_sid = xph.udkey_5_sid
INNER JOIN		c_udkey_7 cu7 on cu7.udkey_7_sid = xph.udkey_7_sid
--where xph.udkey_1_sid = 1968726208
GROUP BY   cu1.udkey_1_id , xph.start_user_date , xph.end_user_date ,  cu7.descr
)
select Product_ID,Licence_Start_Date,Licence_End_Date,Media,Total_Months,Clarence_Pymt_Required,Total_Sales

, CASE WHEN  Clarence_Pymt_Required = 'TRUE' THEN  ((Total_Months - 84) / Total_Months) * Total_Sales 

ELSE 0 END AS Sales_Portion_Over_84_Months
from cte</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
